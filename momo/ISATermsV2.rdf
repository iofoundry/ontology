	<note xmlns="http://www.w3.org/2004/02/skos/core#">should have corresponding role to define this class </note>
	<source xmlns="http://purl.org/dc/terms/">ISA-88 Part 1 Section 3 </source>
</rdf:Description>

<rdf:Description rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3UnitProcedure">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<label xmlns="http://www.w3.org/2000/01/rdf-schema#">Unit Procedure</label>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/PlanSpecification"/>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3"/>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ISA88"/>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ProcessUC"/>
