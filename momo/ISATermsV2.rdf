		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ISA88"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ProcessUC"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3"/>
		<label>Site</label>
		<zzzns0001:source>ISA-88 Part 1 Section 3</zzzns0001:source>
		<zzzns0003:naturalLanguageDefinition>A component of a batch manufacturing enterprise that is identified by physical, geographical, or logical segmentation within the enterprise. It may contain areas, process cells, units, equipment modules, and control modules.</zzzns0003:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3SiteRecipe">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ISA88"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ProcessUC"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3"/>
