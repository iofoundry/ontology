</rdf:Description>

<rdf:Description rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3ControlModule">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
