		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part3Section9"/>
		<label>Inventory response</label>
		<zzzns0001:source>ISA-95 Part 3 Section 9</zzzns0001:source>
		<zzzns0003:naturalLanguageDefinition>The response to an inventory request, indicating the completion status (successful or unsuccessful) of the request</zzzns0003:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part3Section9InventoryStorageDefinitions">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part3Section9"/>
		<label>Inventory storage definitions</label>
		<zzzns0001:source>ISA-95 Part 3 Section 9</zzzns0001:source>
		<zzzns0003:naturalLanguageDefinition>Storage definition information sent to Level 2 associated with movement and control</zzzns0003:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part3Section9InventoryTracking">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part3Section9"/>
