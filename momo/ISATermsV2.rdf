				<owl:someValuesFrom rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3UnitProcedure"/>
			</owl:Restriction>
		</subClassOf>
		<label>Recipe Procedure</label>
		<zzzns0001:source>ISA-88 Part 1 Section 3</zzzns0001:source>
		<zzzns0003:naturalLanguageDefinition>The part of a recipe that defines the strategy for producing a batch.</zzzns0003:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3RecipeProcess">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#HybridUC"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ProcessUC"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/PlanSpecification"/>
		<subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000178"/>
				<owl:someValuesFrom rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part3ProcessStage"/>
			</owl:Restriction>
		</subClassOf>
		<label>Recipe Process</label>
		<zzzns0001:source>ISA-88 Part 1 Section 3</zzzns0001:source>
