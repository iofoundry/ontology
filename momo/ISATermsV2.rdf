					<owl:Restriction>
						<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/hasRole"/>
						<owl:someValuesFrom rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#WorkCenterRole"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
