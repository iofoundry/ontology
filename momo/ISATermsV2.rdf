		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ISA95"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part1Section3"/>
		<label>Production Control</label>
		<zzzns0001:source>ISA-95 Part 1 Section 3</zzzns0001:source>
		<zzzns0003:naturalLanguageDefinition>collection of functions that manages all production within a site or area</zzzns0003:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part1Section3ProductionLine">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#DiscreteUC"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#HybridUC"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ISA95"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part1Section3"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part1Section3WorkCenter"/>
		<label>Production Line</label>
		<zzzns0001:source>ISA-95 Part 1 Section 3</zzzns0001:source>
		<zzzns0003:naturalLanguageDefinition>work center that is a series of pieces of equipment dedicated to the manufacture of a specific number of products or families</zzzns0003:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part1Section3ProductionOperationsManagement">
