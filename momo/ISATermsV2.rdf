							<rdf:Description rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3GeneralRecipe">
							</rdf:Description>
							<rdf:Description rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3SiteRecipe">
