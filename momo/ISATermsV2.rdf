	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part4(Actual)ControlRecipe">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ProcessUC"/>
