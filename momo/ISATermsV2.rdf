	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ISA88"/>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ProcessUC"/>
	<naturalLanguageDefinition xmlns="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/">a set of produced materials that are related by manufacturing business policy.</naturalLanguageDefinition>
	<source xmlns="http://purl.org/dc/terms/">ISA-88 Part 3  </source>
</rdf:Description>

<rdf:Description rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part3ProductGrade">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<label xmlns="http://www.w3.org/2000/01/rdf-schema#">product grade</label>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part3"/>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ISA88"/>
