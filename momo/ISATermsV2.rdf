	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3"/>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part1Section3WorkUnit"/>
