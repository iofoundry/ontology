	<source xmlns="http://purl.org/dc/terms/">ISA-95 Part 2 Section 6 6.2 Operations Schedule Information</source>
</rdf:Description>

<rdf:Description rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part2Section66.2OperationsScheduleInformationSegmentRequirement">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
