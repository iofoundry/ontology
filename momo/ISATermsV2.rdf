<rdf:Description rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-95Part2Section55.4MaterialInformationMaterialLot">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
