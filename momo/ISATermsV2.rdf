	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part2RecipeEntity">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ProcessUC"/>
