		<subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000178"/>
				<owl:onClass rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part1Section3Phase"/>
				<owl:minQualifiedCardinality rdf:datatype="&xs;nonNegativeInteger">0</owl:minQualifiedCardinality>
