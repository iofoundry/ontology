		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part2"/>
		<label>Part 2</label>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part2AllocationSymbol">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part2"/>
		<label>allocation symbol</label>
		<zzzns0001:source>ISA-88 Part 2</zzzns0001:source>
		<zzzns0003:naturalLanguageDefinition>a graphical symbol that is used to represent the encapsulation of the resource allocation and de-allocation
rules for a recipe procedural element.</zzzns0003:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/glossary/isaterms/ISA-88Part2BatchHistory">
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#HybridUC"/>
		<subClassOf rdf:resource="https://spec.industrialontologies.org/glossary/isaterms#ProcessUC"/>
