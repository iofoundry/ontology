	<label xmlns="http://www.w3.org/2000/01/rdf-schema#">product family</label>
	<example xmlns="http://www.w3.org/2004/02/skos/core#">mAb, mRNA, orange juice family, iPhone 16</example>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/InformationContentEntity"/>
