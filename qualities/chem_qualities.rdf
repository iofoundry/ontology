    <!-- http://purl.obolibrary.org/obo/BFO_0000145 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000145"/>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Adhesion -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Adhesion">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Pressure"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Adhesion</rdfs:label>
        <rdfs:label xml:lang="de">Adhäsion</rdfs:label>
        <skos:definition xml:lang="en">Adhesion is a relative quality that describes the ability of substances to bind to each other. It involves the attractive force between two bodies of different substances when they come into contact. This property is crucial in various scientific and industrial contexts, including material science, biology, and engineering. The degree of adhesion can significantly influence the performance of materials and products, such as coatings, glues, tapes, and biological cells.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/ChemicalQuality -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/ChemicalQuality">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Chemical Quality</rdfs:label>
        <rdfs:label xml:lang="de">Chemische Qualität</rdfs:label>
        <skos:definition xml:lang="en">Chemical Quality is a quality that pertains to the properties or characteristics of an entity concerning its chemical condition or chemical categorization. It may encompass a variety of aspects such Electron Negativity, Molar Mass, etc.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/ChemicalRelationalQuantity -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/ChemicalRelationalQuantity">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000145"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <rdfs:label xml:lang="en">Chemical Relational Quality</rdfs:label>
        <rdfs:label xml:lang="de">Chemische Relationale Qualität</rdfs:label>
        <skos:definition xml:lang="en">Chemical Relational Quality is a relational quality that pertains to the properties or characteristics of an entity related to other entities, which depend on that second entity to exist. It may encompass a variety of aspects like solubability or affinity.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/ClampingPressure -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/ClampingPressure">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Pressure"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="en">Clamping Pressure</rdfs:label>
        <rdfs:label xml:lang="de">Klemmdruck</rdfs:label>
        <skos:definition xml:lang="de">Diese Eigenschaft beschreibt als quantitativer Wert die Kraft pro Fläche (Druck), die beim Einbau einer Probe, eines Prüfkörpers oder eines Prüfstücks aufgebracht wird. Typischerweise ist das Aufbringen eines Klemmdrucks mit einer Art von Klemm- oder Spannzubehör (Grips) oder Ähnlichem verbunden.</skos:definition>
        <skos:definition xml:lang="en">This property describes the force per area (pressure) that is used during the mounting process of a sample, specimen, or test piece as a quantitative value. Typically, this process involves some sort of grips or the like.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Concentration -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Concentration">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/MaterialComposition"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Concentration</rdfs:label>
        <rdfs:label xml:lang="de">Konzentration</rdfs:label>
        <skos:altLabel xml:lang="de">Konzentration</skos:altLabel>
        <skos:prefLabel xml:lang="en">Concentration</skos:prefLabel>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/CreepStress -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/CreepStress">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Stress"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="en">Creep Stress</rdfs:label>
        <rdfs:label xml:lang="de">Kriechspannung</rdfs:label>
        <skos:definition xml:lang="en">Is the strain occuring with time, when a constant force is applied to a physical body.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Density -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Density">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Potential"/>
        <rdfs:label xml:lang="en">Density</rdfs:label>
        <rdfs:label xml:lang="de">Dichte</rdfs:label>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/ElectricCurrent -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/ElectricCurrent">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Potential"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Electric Current</rdfs:label>
        <rdfs:label xml:lang="de">Elektrischer Strom</rdfs:label>
        <skos:altLabel xml:lang="de">Elektischer Strom</skos:altLabel>
        <skos:prefLabel xml:lang="en">Electric Current</skos:prefLabel>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/EngineeringStress -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/EngineeringStress">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Stress"/>
        <obo:IAO_0000116 xml:lang="en">Assumes unchanged size of Area the force is normalized to. Changes in the Area can only be neclected for very small deformations.</obo:IAO_0000116>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Engineering Stress</rdfs:label>
        <rdfs:label xml:lang="de">Technische Spannung</rdfs:label>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Force -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Force">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Potential"/>
        <obo:IAO_0000119 xml:lang="en">“Force.” Merriam-Webster.com Dictionary, Merriam-Webster, https://www.merriam-webster.com/dictionary/force. Accessed 5 Dec. 2022.</obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="en">Force</rdfs:label>
        <rdfs:label xml:lang="de">Kraft</rdfs:label>
        <skos:definition xml:lang="en">Strength or energy exerted or brought to bear : cause of motion or change : active power.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Humidity -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Humidity">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Concentration"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="de">Feuchtigkeit</rdfs:label>
        <rdfs:label xml:lang="en">Humidity</rdfs:label>
        <skos:definition xml:lang="en">A quality inhering in air by virtue of the partial pressure exerted by the bearer&apos;s water vapour content.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Hydrophilic -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Hydrophilic">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Hydrophilicity"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <obo:IAO_0000412>http://purl.obolibrary.org/obo/PATO_0001887</obo:IAO_0000412>
        <rdfs:label xml:lang="de">Hydrophil</rdfs:label>
        <rdfs:label xml:lang="en">Hydrophilic</rdfs:label>
        <skos:definition xml:lang="en">A quality inhering in a bearer by virtue of the bearer&apos;s disposition to having a strong affinity for water; tending to dissolve in, mix with, or be wetted by water.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Hydrophilicity -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Hydrophilicity">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/ChemicalRelationalQuantity"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <obo:IAO_0000412>http://purl.obolibrary.org/obo/PATO_0002455</obo:IAO_0000412>
        <rdfs:label xml:lang="en">Hydrophilicity</rdfs:label>
        <rdfs:label xml:lang="de">Hydrophilität</rdfs:label>
        <skos:definition xml:lang="en">A physical quality inhering in a bearer by virtue the bearer&apos;s disposition to having an affinity for water; it is readily absorbing or dissolving in water.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Hydrophobic -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Hydrophobic">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Hydrophilicity"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <obo:IAO_0000412>http://purl.obolibrary.org/obo/PATO_0001885</obo:IAO_0000412>
        <rdfs:label xml:lang="de">Hydrophob</rdfs:label>
        <rdfs:label xml:lang="en">Hydrophobic</rdfs:label>
        <skos:definition xml:lang="en">A quality inhering in a bearer by virtue of the bearer&apos;s lacking affinity for water; tending to repel and not absorb water; tending not to dissolve in or mix with or be wetted by water.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/LoadCellCapacity -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/LoadCellCapacity">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Force"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="de">Kapazität Der Wägezelle</rdfs:label>
        <rdfs:label xml:lang="en">Load Cell Capacity</rdfs:label>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/MassPotential -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/MassPotential">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Potential"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <rdfs:label xml:lang="en">Mass Potential</rdfs:label>
        <rdfs:label xml:lang="de">Massenpotential</rdfs:label>
        <skos:definition xml:lang="en">Mass potential is a physical potential, which is a scalar quantity that describes the energy stored in a material due to its position, shape, or configuration. It is a measure of the gravitational potential energy of a mass relative to some reference point. Mass potential is expressed in units of energy per unit mass or energy per unit volume.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/MaterialComposition -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/MaterialComposition">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Potential"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Material Composition</rdfs:label>
        <rdfs:label xml:lang="de">Materialzusammensetzung</rdfs:label>
        <skos:definition xml:lang="en">Material Composition is a relative quality that describes the ratio of different substances or components that make up a material or a representative volume element of it. This property can include aspects such as the types of atoms, molecules, or compounds present, their proportions, and the way they are arranged.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/MolecularQuality -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/MolecularQuality">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/ChemicalQuality"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <obo:IAO_0000117 xml:lang="en">PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Molecular Quality</rdfs:label>
        <rdfs:label xml:lang="de">Molekulare Qualität</rdfs:label>
        <skos:definition xml:lang="en">A quality which inheres in a molecular entity, a single molecule, atom, ion, radical etc.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/NumberDensity -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/NumberDensity">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Density"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Number Density</rdfs:label>
        <rdfs:label xml:lang="de">Stoffmenge</rdfs:label>
        <skos:definition xml:lang="en">Number density is a measure of the number of objects in a related unit volume, or unit area.</skos:definition>
        <skos:prefLabel xml:lang="en">Number Density</skos:prefLabel>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/PhysicalRelationalQuality -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/PhysicalRelationalQuality">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000145"/>
        <rdfs:label xml:lang="en">Physical Relational Quality</rdfs:label>
        <rdfs:label xml:lang="de">Physisch Relative Qualität</rdfs:label>
        <skos:definition xml:lang="en">Pyhsical Relational Quality is a relational quality that pertains to the properties or characteristics of an entity related to other entities, which depend on that second entity to exist. It may encompass a variety of aspects like physical potentials between bodys.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/PixelDensity -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/PixelDensity">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/NumberDensity"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="en">Pixel Density</rdfs:label>
        <rdfs:label xml:lang="de">Pixeldichte</rdfs:label>
        <skos:definition xml:lang="en">Pixel density can be defined as a physical relational quality that describes the relationship between the number of pixels and the area they occupy on a related entity.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Potential -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Potential">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/PhysicalRelationalQuality"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="en">Potential</rdfs:label>
        <rdfs:label xml:lang="de">Potenzial</rdfs:label>
        <skos:definition xml:lang="en">Potential is a relative quality that describes the capacity of an entity to perform work or transform in a system due to its position or configuration. This can refer to various types of potentials such as gravitational, electric, or chemical potential, among others. The potential represents a stored energy that can be converted into kinetic energy or work under suitable conditions. It is a fundamental concept in various branches of physics and engineering.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Pressure -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Pressure">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Potential"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="de">Druck</rdfs:label>
        <rdfs:label xml:lang="en">Pressure</rdfs:label>
        <skos:definition xml:lang="en">Pressure is defined as the amount of force applied per unit area. It is often expressed in units of pounds per square inch (psi), pascals (Pa), or atmospheres (atm). Pressure can be thought of as a measure of the potential energy stored in a fluid or gas.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Stress -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Stress">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Pressure"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="de">Spannung</rdfs:label>
        <rdfs:label xml:lang="en">Stress</rdfs:label>
        <skos:definition xml:lang="en">Stress is defined as the internal force per unit area (pressure) that a material experiences when it is subjected to an external load or force. Stress can be tensile (pulling apart), compressive (squeezing together), or shear (sliding past each other) and is commonly used to describe the behavior of solids.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/SurfaceTension -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/SurfaceTension">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Adhesion"/>
        <obo:IAO_0000117>PERSON: Thomas Hanke</obo:IAO_0000117>
        <rdfs:label xml:lang="de">Oberflächenspannung</rdfs:label>
        <rdfs:label xml:lang="en">Surface Tension</rdfs:label>
        <skos:altLabel xml:lang="en">Surface Stress</skos:altLabel>
        <skos:definition xml:lang="de">Die Oberflächenspannung einer Flüssigkeit ist ein Effekt innerhalb der Oberflächenschicht der Flüssigkeit, der durch Wechselwirkungskräfte zwischen den Molekülen der Flüssigkeit verursacht wird.</skos:definition>
        <skos:definition xml:lang="en">The surface tension of a liquid is an effect within the surface layer of the liquid caused by intermolecular forces between the molecules of the liquid.</skos:definition>
        <skos:prefLabel xml:lang="de">Oberflächenspannung</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Surface Tension</skos:prefLabel>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/TrueStress -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/TrueStress">
