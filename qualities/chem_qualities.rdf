        <dcterms:title xml:lang="en-us">Industrial Ontology Foundry (IOF) Qualities Ontology</dcterms:title>
        <rdfs:label xml:lang="en-us">Qualities Ontology</rdfs:label>
        <iof-av:acronym>QO</iof-av:acronym>
        <iof-av:copyright>Copyright (c) 2022 Open Applications Group</iof-av:copyright>
        <iof-av:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Provisional"/>
    </owl:Ontology>
    


