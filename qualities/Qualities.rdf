<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY dc "http://purl.org/dc/elements/1.1/">
	<!ENTITY dcterms "http://purl.org/dc/terms/">
	<!ENTITY iof-av "https://spec.industrialontologies.org/ontology/annotation/">
	<!ENTITY iof-const "https://spec.industrialontologies.org/ontology/construct/">
	<!ENTITY iof-ind "https://spec.industrialontologies.org/ontology/individual/">
	<!ENTITY obo "http://purl.obolibrary.org/obo/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY prov "http://www.w3.org/ns/prov#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.industrialontologies.org/ontology/qualities/Qualities/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:iof-av="https://spec.industrialontologies.org/ontology/annotation/"
	xmlns:iof-const="https://spec.industrialontologies.org/ontology/construct/"
	xmlns:iof-ind="https://spec.industrialontologies.org/ontology/individual/"
	xmlns:obo="http://purl.obolibrary.org/obo/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:prov="http://www.w3.org/ns/prov#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities/">
		<rdfs:label xml:lang="en">Qualities Ontology (base)</rdfs:label>
		<dcterms:abstract xml:lang="en">The IOF (common) Quality Ontology is an architectural construct allowing separate Quality ontologies for different domains e.g., Physical, Chemical, Biological, etc.
        This ontology also contains those qualities that may be considered &apos;common&apos;.</dcterms:abstract>
		<dcterms:contributor xml:lang="en-US">Thomas Hanke, Fraunhofer IWM</dcterms:contributor>
		<dcterms:creator xml:lang="en-US">IOF Materials Working Group</dcterms:creator>
		<dcterms:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dcterms:license>
		<dcterms:publisher>Industrial Ontology Foundry</dcterms:publisher>
		<dcterms:references rdf:resource="http://spec.org/dc/terms/"/>
		<dcterms:references rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
		<dcterms:title xml:lang="en">Industrial Ontology Foundry (IOF) Qualities Ontology (Base)</dcterms:title>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/"/>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/"/>
		<owl:versionIRI rdf:resource="https://spec.industrialontologies.org/ontology/202507/qualities/Qualities/"/>
		<iof-av:copyright xml:lang="en">Industrial Ontology Foundry (IOF)</iof-av:copyright>
		<iof-av:maturity rdf:resource="&iof-ind;Provisional"/>
	</owl:Ontology>
	
	<owl:Class rdf:about="&iof-const;Action">
		<rdfs:subClassOf rdf:resource="&obo;BFO_0000015"/>
		<rdfs:label xml:lang="de">Aktion</rdfs:label>
		<rdfs:label xml:lang="en">action</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/qualities/Qualities/</rdfs:isDefinedBy>
		<owl:equivalentClass>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Restriction>
						<owl:onProperty rdf:resource="&obo;BFO_0000057"/>
						<owl:someValuesFrom>
							<owl:Class>
								<owl:intersectionOf rdf:parseType="Collection">
									<rdf:Description rdf:about="&obo;BFO_0000040">
									</rdf:Description>
									<owl:Restriction>
										<owl:onProperty rdf:resource="&iof-const;hasQuality"/>
										<owl:someValuesFrom rdf:resource="&obo;BFO_0000145"/>
									</owl:Restriction>
								</owl:intersectionOf>
							</owl:Class>
						</owl:someValuesFrom>
					</owl:Restriction>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&iof-const;hasProcessCharacteristic"/>
						<owl:someValuesFrom rdf:resource="&iof-const;ProcessCharacteristic"/>
					</owl:Restriction>
				</owl:unionOf>
			</owl:Class>
		</owl:equivalentClass>
		<iof-av:naturalLanguageDefinition xml:lang="en">Action is a process introducing a change as a response or interaction in a participating material entity</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&iof-const;DimensionQuality">
		<rdfs:subClassOf rdf:resource="&obo;BFO_0000019"/>
		<rdfs:label xml:lang="en">dimension quality</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/qualities/Qualities/</rdfs:isDefinedBy>
	</owl:Class>
	
	<owl:Class rdf:about="&iof-const;Reaction">
		<rdfs:subClassOf rdf:resource="&obo;BFO_0000015"/>
		<rdfs:label xml:lang="de">Reaktion</rdfs:label>
		<rdfs:label xml:lang="en">reaction</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/qualities/Qualities/</rdfs:isDefinedBy>
		<owl:equivalentClass>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-const;isTriggeredBy"/>
				<owl:someValuesFrom rdf:resource="&iof-const;Action"/>
			</owl:Restriction>
		</owl:equivalentClass>
		<skos:altLabel xml:lang="de">Antwort</skos:altLabel>
		<iof-av:naturalLanguageDefinition xml:lang="en">Reaction represents a process resulting in a realizable entity being realized in response to an action</iof-av:naturalLanguageDefinition>
		<iof-av:naturalLanguageDefinition xml:lang="de">Reaktion stellt einen Prozess dar, der zu einem realisierbaren Wesen führt, das als Antwort auf eine Aktion realisiert wird</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&iof-const;SpatialRelationalQuality">
		<rdfs:subClassOf rdf:resource="&obo;BFO_0000145"/>
		<rdfs:label xml:lang="en">spatial relational quality</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/qualities/Qualities/</rdfs:isDefinedBy>
	</owl:Class>
	
	<owl:Class rdf:about="&iof-const;ToBeReviewed">
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/qualities/Qualities/</rdfs:isDefinedBy>
		<skos:note xml:lang="en">This is a temporary class to hold those constructs that need additional review.</skos:note>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&iof-const;isTriggeredBy">
		<rdfs:label xml:lang="de">ausgelöst durch</rdfs:label>
		<rdfs:label xml:lang="en">is triggered by</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/qualities/Qualities/</rdfs:isDefinedBy>
		<owl:inverseOf rdf:resource="&iof-const;triggers"/>
		<iof-av:naturalLanguageDefinition xml:lang="en">inverse if triggers</iof-av:naturalLanguageDefinition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&iof-const;triggers">
		<rdfs:label xml:lang="en">triggers</rdfs:label>
		<rdfs:label xml:lang="de">verursacht</rdfs:label>
		<rdfs:domain rdf:resource="&iof-const;Action"/>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/qualities/Qualities/</rdfs:isDefinedBy>
		<rdfs:range rdf:resource="&iof-const;Reaction"/>
		<iof-av:naturalLanguageDefinition xml:lang="en">A stimulus process X can trigger one or more response processes that realize a realizable entity.</iof-av:naturalLanguageDefinition>
	</owl:ObjectProperty>

</rdf:RDF>