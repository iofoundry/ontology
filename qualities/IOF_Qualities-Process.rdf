    <!-- https://spec.industrialontologies.org/ontology/qualities/Qualities-Process/CalibrationRange -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-Process/CalibrationRange">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Qualities/ToBeReviewed"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="en">Calibration Range</rdfs:label>
        <rdfs:label xml:lang="de">Kalibrierbereich</rdfs:label>
        <skos:definition xml:lang="de">Diese Eigenschaft beschreibt den Bereich zwischen den Grenzen, innerhalb derer eine Größe gemessen, empfangen oder übertragen wird (typischerweise von einem Prozessknoten, z. B. einer Maschine), ausgedrückt durch die Angabe des unteren und oberen Bereichswertes.</skos:definition>
        <skos:definition xml:lang="en">This property describes the region between the limits within which a quantity is measured, received or transmitted (typically by a process node, e.g. a machine), expressed by stating the lower and upper range values.</skos:definition>
        <skos:note xml:lang="en">Was a subclass of &apos;Process Quality&apos;</skos:note>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Qualities-Process/DataAcquisitionRate -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-Process/DataAcquisitionRate">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Qualities/ToBeReviewed"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="en">Data Acquisition Rate</rdfs:label>
        <rdfs:label xml:lang="de">Datenerfassungsrate</rdfs:label>
        <skos:definition xml:lang="de">Diese Eigenschaft bezeichnet die Anzahl der Messungen / Abfragen pro Zeiteinheit bei einer betrachteten Messung einer Messgröße (Abtast-/Abfragerate bei der Datenerfassung).</skos:definition>
        <skos:definition xml:lang="en">This property denotes the number of measurements / queries per time period for a considered measurement of a measurement parameter (sampling / query rate during data acquisition).</skos:definition>
        <skos:note xml:lang="en">Was a subclass of &apos;Process Quality&apos;</skos:note>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/qualities/Qualities-Process/Precision -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/qualities/Qualities-Process/Precision">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/qualities/Qualities/ToBeReviewed"/>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000428"/>
        <obo:IAO_0000119 xml:lang="en">“Precision.” Merriam-Webster.com Dictionary, Merriam-Webster, https://www.merriam-webster.com/dictionary/precision. Accessed 13 Jan. 2023.</obo:IAO_0000119>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/pmd/co"/>
        <rdfs:label xml:lang="de">Genauigkeit</rdfs:label>
        <rdfs:label xml:lang="en">Precision</rdfs:label>
        <skos:definition xml:lang="de">Der Grad der Verfeinerung, mit dem eine Operation durchgeführt oder eine Messung angegeben wird.</skos:definition>
        <skos:definition xml:lang="en">The degree of refinement with which an operation is performed or a measurement stated.</skos:definition>
        <skos:note xml:lang="en">Was a subclass of &apos;Process Quality&apos;</skos:note>
    </owl:Class>
    


