##
# banner No label should contain multiple embedded spaces.



SELECT DISTINCT ?warning ?resource
WHERE 
{
	?resource ?property ?value.
	FILTER (ISLITERAL(?value))
	FILTER (REGEX(?value, "[^\\s]+[\u0020\\t][\u0020\t]+[^\\s]+"))
	
	FILTER (REGEX(str(?resource), <HYGIENE_TESTS_FILTER_PARAMETER>))
	
	BIND (concat ("EDMC_WARN: ", "Resource ", STR(?resource), " has property ", STR(?property), " whose value contains multiple embedded spaces.") AS ?warning)
}
