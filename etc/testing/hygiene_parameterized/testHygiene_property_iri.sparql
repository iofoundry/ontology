PREFIX owl:   <http://www.w3.org/2002/07/owl#>
PREFIX xsd:   <http://www.w3.org/2001/XMLSchema#>
PREFIX rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#> 


##
# banner IOF IRI should follow https://oagi.atlassian.net/wiki/x/AYBzYwE


SELECT DISTINCT ?warning ?resource
WHERE
{
    ?resource rdf:type ?type.
    VALUES ?type {owl:ObjectProperty owl:DatatypeProperty}

    FILTER (!regex(str(?resource), 'https:\\/\\/[a-z\\/\\.]+\\/ontology\\/construct\\/[a-z]+([A-Z][a-z]+)*'))

    FILTER regex(str(?resource), <HYGIENE_TESTS_FILTER_PARAMETER>)

    BIND (CONCAT("WARN: IRI for property ",STR(?resource)," may be ill-formed") as ?warning)
}