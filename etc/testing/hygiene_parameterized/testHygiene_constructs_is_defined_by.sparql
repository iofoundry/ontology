PREFIX owl:   <http://www.w3.org/2002/07/owl#> 
PREFIX rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#> 


##
# banner All constructs must have a rdfs:isDefinedBy annotation.


SELECT DISTINCT ?warning ?resource
WHERE 
{
  ?resource rdf:type ?type .
  VALUES ?type {owl:Class owl:AnnotationProperty owl:ObjectProperty owl:DatatypeProperty}

  FILTER regex(str(?resource), <HYGIENE_TESTS_FILTER_PARAMETER>)	

  FILTER NOT EXISTS {?resource rdfs:isDefinedBy ?definition.}

  BIND (CONCAT("PRODERROR: Construct ", STR(?resource), " does not have a rdfs:isDefinedBy annotation.") AS ?warning)
}
