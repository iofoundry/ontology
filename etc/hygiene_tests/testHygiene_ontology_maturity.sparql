PREFIX owl:   <http://www.w3.org/2002/07/owl#> 
PREFIX rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX xsd:   <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX dct: <http://purl.org/dc/terms/>

PREFIX iof-av: <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/>

##
# banner An ontology must have a maturity level.


SELECT DISTINCT ?warning
WHERE 
{
  ?ont a owl:Ontology .
  FILTER NOT EXISTS {?ont owl:deprecated "true"^^xsd:boolean.}
  FILTER NOT EXISTS {?ont iof-av:maturity ?maturityLevel.}

  FILTER regex(str(?ont), <HYGIENE_TESTS_FILTER_PARAMETER>)	

  BIND (concat ("PRODERROR:: Ontology ", STR(?ont), " does not have a maturity level defined.") AS ?warning)
}
