##
# banner No label should contain trailing spaces.



SELECT DISTINCT ?warning ?value
WHERE
{
	?resource ?property ?value.
	FILTER (ISLITERAL(?value))
	FILTER (REGEX(?value, '.+[\u0020\t]+$'))
	
	FILTER regex(str(?resource), <HYGIENE_TESTS_FILTER_PARAMETER>)
	
	BIND (concat ("PRODERROR: ", "Resource <", STR(?resource), "> has property <", STR(?property), "> whose value contains trailing space(s).") AS ?warning)
}
