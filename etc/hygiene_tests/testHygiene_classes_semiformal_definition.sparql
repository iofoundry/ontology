PREFIX rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:  <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl:   <http://www.w3.org/2002/07/owl#>
PREFIX xsd:   <http://www.w3.org/2001/XMLSchema#>

PREFIX iof-av: <https://purl.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/>


##
# banner Every non-primitive class must have a semi-formal natural language definition.


SELECT DISTINCT ?warning ?class
WHERE
{
  ?class rdf:type owl:Class.
  FILTER NOT EXISTS {?class iof-av:isPrimitive "f"^^xsd:boolean.}
  
  FILTER NOT EXISTS {?class iof-av:semiFormalNaturalLanguageDefinition ?definition.}
  
  FILTER REGEX(STR(?class), <HYGIENE_TESTS_FILTER_PARAMETER>)
  
  BIND (CONCAT("PRODERROR: A non-primitive class ", STR(?class), " does not have a semi-formal natural language definition.") as ?warning)
}
