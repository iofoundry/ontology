##
# banner No label should contain leading spaces.


SELECT DISTINCT ?warning ?value
WHERE
{
	?resource ?property ?value.
	FILTER (ISLITERAL(?value))
	FILTER (REGEX(?value, '^[\u0020\t]+.+'))
	
	FILTER regex(str(?resource), <HYGIENE_TESTS_FILTER_PARAMETER>)
	
	BIND (concat ("WARNING: ", "Resource <", STR(?resource), "> has property <", STR(?property), "> whose value contains leading space(s).") AS ?warning)
}
