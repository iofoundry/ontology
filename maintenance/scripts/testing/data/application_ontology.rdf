<owl:Class rdf:about="WorkFromInspection">
  <rdfs:subClassOf rdf:resource="MaintenanceStrategy"/>
</owl:Class>

<owl:Class rdf:about="WorkFromBreakdown">
  <rdfs:subClassOf rdf:resource="MaintenanceStrategy"/>
</owl:Class>

