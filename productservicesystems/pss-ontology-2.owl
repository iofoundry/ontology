    <!-- https://purl.industrialontologies.org/ontology/core/Core/CommercialServiceSpecification -->

    <rdf:Description rdf:about="https://purl.industrialontologies.org/ontology/core/Core/CommercialServiceSpecification">
        <rdfs:subClassOf>
