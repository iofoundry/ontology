				<owl:someValuesFrom rdf:resource="&iof-const;ProductServiceSystem"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">product service system supplier organization</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/productservicesystems/ProductServiceSystem/</rdfs:isDefinedBy>
		<skos:altLabel xml:lang="en-US">PSS provider organization</skos:altLabel>
		<skos:altLabel xml:lang="en-US">PSS vendor organization</skos:altLabel>
		<iof-av:abbreviation xml:lang="en-US">PSS supplier organization</iof-av:abbreviation>
		<iof-av:explanatoryNote>Additional subclasses are possible, e.g. in material supplier, component supplier etc.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">business organisation that both produces and sells something (product, service, PSS), or only sells it</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&iof-const;ProductServiceSystemType">
		<rdfs:subClassOf rdf:resource="&bfo;BFO_0000027"/>
