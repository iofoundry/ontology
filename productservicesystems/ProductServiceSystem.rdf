    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Benefit -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Benefit">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Indicator"/>
        <rdfs:label xml:lang="en-us">benefit</rdfs:label>
        <skos:example>Examples could be e.g. cost reduction, eco friendliness, performance improvement, risk reduction</skos:example>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">an indicator that depicts a positive value (e.g. financial, satisfaction, assistance, etc.)  a stakeholder (e.g. a customer) receives back as a result of the offered PSS</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#BusinessModel -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#BusinessModel">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <rdfs:comment>This is an interface class to other ontologies - NLD to be revisited, might not be in the correct place in the ontology</rdfs:comment>
        <rdfs:label xml:lang="en-us">business model</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">a description of a plan and implementation of the plan, aiming the successful operation of a business</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#BusinessResourceRole -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#BusinessResourceRole">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <rdfs:label xml:lang="en-us">business resource role</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">business resource role is a role that inheres in an independent continuant where that continuant bears a disposition that, if realized, is realized in a business enterprise process</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#CommercialServiceComponent -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#CommercialServiceComponent">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ProcessDefinition"/>
        <core1:xUnresolvedIssues>as of 10.05.2022: we are substituting &apos;service offer&apos; by &apos;commercial service definition&apos; therefore we need to review this entity</core1:xUnresolvedIssues>
        <rdfs:label xml:lang="en-us">commercial service component</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">a process definition which is part of a commercial service specification. It can be itself a service, but for the offered service within the scope of PSS, its existence only has a meaning as part of the commercial service</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#CommercialServiceIntegratedSet -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#CommercialServiceIntegratedSet">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ProcessDefinition"/>
        <core1:xUnresolvedIssues>as of 10.05.2022: we are substituting &apos;service offer&apos; by &apos;commercial service definition&apos; therefore we need to review this entity</core1:xUnresolvedIssues>
        <rdfs:label xml:lang="en-us">commercial service integrated set</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">a process definition that refers to a set of commercial service specification, needed to jointly fulfil users’ needs.</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ConsumableResourceRole -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ConsumableResourceRole">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <core1:xUnresolvedIssues>as of 10.05.2022: adapted due to clean up of the ontology, check NLD</core1:xUnresolvedIssues>
        <rdfs:label xml:lang="en-us">consumable resource role</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">role that is borne by a material entity, when that entity is intended to be consumed/transformed in a planned process. The quantities of resources is reduced after use</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#DigitalLogData -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#DigitalLogData">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ProductData"/>
        <rdfs:label xml:lang="en-us">digital log data</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">product data generated from a software element, part of the product</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#EmbeddedData -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#EmbeddedData">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ProductData"/>
        <rdfs:label xml:lang="en-us">embedded data</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">product data generated from embedded data-source components (e.g. sensors)</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#EmbeddedDataSourceComponent -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#EmbeddedDataSourceComponent">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#PSSProductComponent"/>
        <rdfs:label xml:lang="en-us">embedded data source component</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">a PSS product component that refers to product-embedded information devices / parts (e.g. sensors), which allows for (additional) services to be offered for the product</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Employee -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Employee">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#IndividualPerson"/>
        <rdfs:label xml:lang="en-us">employee</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">individual person that works in a business organization</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Feature -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Feature">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#PSSInformation"/>
        <core1:xUnresolvedIssues>Look for IOF:Feature. At the moment it&apos;s a deprecated term</core1:xUnresolvedIssues>
        <rdfs:label xml:lang="en-us">feature</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">PSS Information that describes the characteristics of a product, service, or PSS</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Feedback -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Feedback">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#PSSInformation"/>
        <rdfs:label xml:lang="en-us">feedback</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">PSS Information that represents any opinion on PSS, product or service (e.g. comments on social media, opinion expression in online questionnaires, etc.), coming from the Stakeholders (e.g., Business or Individual Customers)</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Firmware -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Firmware">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#PSSProductComponent"/>
        <rdfs:label xml:lang="en-us">firmware</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">a PSS Product Component which is software, i.e. control software of a machine that exists only in combination with specific hardware product</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Guideline -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Guideline">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/ActionSpecification"/>
        <rdfs:label xml:lang="en-us">guideline</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">action specification referring to a statement describing a course of action aiming to reach a specific goal for the design, development or use of a PSS</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#HardwareInfrastructure -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#HardwareInfrastructure">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/MaterialArtifact"/>
        <core1:xUnresolvedIssues>15.06.2022 - to be harmonised with facility in the Supply Chain ontology</core1:xUnresolvedIssues>
        <rdfs:label>hardware infrastucture</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">material artifact that is an Infrastructure that refers to the physical networks (resources) necessary for the design, development, delivery and operation of products and/or services</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ImmaterialResourceRole -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ImmaterialResourceRole">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <rdfs:label xml:lang="en-us">immaterial resource role</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">role played by an information content entiy when that entity is intended to be used in a planned process as a resource</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Indicator -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Indicator">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#PSSInformation"/>
        <rdfs:label xml:lang="en-us">indicator</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">PSS Information used to measure/evaluate any aspect within the scope of PSS, namely a process, a PSS, a product, a service, etc.</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#IndividualCustomer -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#IndividualCustomer">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#IndividualPerson"/>
        <CommonCoreOntologies:alternative_label xml:lang="en-us">consumer</CommonCoreOntologies:alternative_label>
        <rdfs:label>individual customer</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">individual person that buys or consumes (consumer) any product and/or service</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#IndividualPerson -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#IndividualPerson">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Stakeholder"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#owns"/>
                <owl:someValuesFrom rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#ProductServiceSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en-us">individual person</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">stakeholder indicating a separate person who is not legal entity (i.e. not legally considered a corporate group, as for example a company)</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Infrastructure -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Infrastructure">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000030"/>
        <rdfs:comment>15.06.2022 - to be harmonised with facility in SCRO
Previous definition to discuss: PSS Infrastructure is a PSS Resource describing the facilities (physical e.g. buildings or machines, and organisational e.g. computer networks or software applications) used to assist in development and delivering of the PSS.
24.08.2022: do we need this?</rdfs:comment>
        <rdfs:label xml:lang="en-us">infrastructure</rdfs:label>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">object describing the facilities (physical e.g. buildings or machines, and organisational e.g. computer networks or software applications) used to assist in development and delivering of a products and/or services</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#KeyPerformanceIndicator -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#KeyPerformanceIndicator">
        <rdfs:subClassOf rdf:resource="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Indicator"/>
        <rdfs:comment>This is an interface class to other ontologies</rdfs:comment>
        <rdfs:label xml:lang="en-us">key performance indicator</rdfs:label>
        <skos:example>Various types of KPIs can be defined: manufacturing KPI, environmental KPI etc.</skos:example>
        <AnnotationVocabulary:acronym>KPI</AnnotationVocabulary:acronym>
        <AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en-us">an indicator referring to (a set of) measurement(s) that evaluates the performance (or success) of a PSS (concept), and is business relevant</AnnotationVocabulary1:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Lifecycle -->

    <owl:Class rdf:about="https://spec.inductrialontologies.org/ontology/productservicesystems/ProducServiceSystem#Lifecycle">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:comment>This is an interface class to other ontologies. Needs to be reworked</rdfs:comment>
