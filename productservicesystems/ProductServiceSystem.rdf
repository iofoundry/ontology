        <iof-av:naturalLanguageDefinition xml:lang="en-us">contains all the parts and assemblies required to build a complete and shippable product</iof-av:naturalLanguageDefinition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.industrialontologies.org/ontology/construct/ManufacturingBillOfMaterials"/>
        <owl:annotatedProperty rdf:resource="https://spec.industrialontologies.org/ontology/annotation/explanatoryNote"/>
        <owl:annotatedTarget>BOM from the production perspective</owl:annotatedTarget>
        <rdfs:comment>https://www.iso.org/obp/ui/#iso:std:iso:18828:-2:ed-1:v1:en</rdfs:comment>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.industrialontologies.org/ontology/construct/ManufacturingBillOfMaterials"/>
        <owl:annotatedProperty rdf:resource="https://spec.industrialontologies.org/ontology/annotation/naturalLanguageDefinition"/>
        <owl:annotatedTarget>contains all the parts and assemblies required to build a complete and shippable product</owl:annotatedTarget>
        <rdfs:comment>https://en.wikipedia.org/wiki/Manufacturing_bill_of_materials</rdfs:comment>
    </owl:Axiom>
    


    <!-- https://spec.industrialontologies.org/ontology/construct/ManufacturingResource -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/construct/ManufacturingResource">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/construct/isInputOf"/>
