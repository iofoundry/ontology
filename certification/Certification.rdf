	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.industrialontologies.org/ontology/certification/Certification/">
		<rdfs:label xml:lang="en-US">Certification Ontology</rdfs:label>
		<dcterms:abstract>TBD</dcterms:abstract>
		<dcterms:contributor xml:lang="en-US">Ali Hasanzadeh, Arizona State University</dcterms:contributor>
		<dcterms:creator xml:lang="en-US">IOF Supply Chain Working Group</dcterms:creator>
		<dcterms:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dcterms:license>
		<dcterms:publisher xml:lang="en-US">Industrial Ontology Foundry</dcterms:publisher>
		<dcterms:title>Certification Ontology (CO)</dcterms:title>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/"/>
		<owl:versionIRI rdf:resource="https://spec.industrialontologies.org/ontology/202401/certification/Certification/"/>
		<iof-av:copyright>Copyright (c) 2022, 2023, Open Applications Group</iof-av:copyright>
		<iof-av:maturity rdf:resource="&iof-ind;Provisional"/>
