    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/AttestingProcess">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/PlannedProcess"/>
