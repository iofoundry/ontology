    <!-- https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/ShipFromLocation -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/ShipFromLocation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
