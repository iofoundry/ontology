    <!-- https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/CertifyingBodyRole -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/CertifyingBodyRole">
