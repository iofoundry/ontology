    <!-- https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/Change -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/Change">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
        <rdfs:label xml:lang="en-us">change</rdfs:label>
        <skos:example>the process of change in the color of an object, the process of gaining a new student role when a person enrolls for the first time in university courses</skos:example>
        <iof-av:directSource>CCO</iof-av:directSource>
        <iof-av:explanatoryNote>candidate IOF Core construct</iof-av:explanatoryNote>
