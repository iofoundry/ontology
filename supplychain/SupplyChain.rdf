		<rdfs:domain rdf:resource="&bfo;BFO_0000015"/>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/</rdfs:isDefinedBy>
		<rdfs:range>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&bfo;BFO_0000006">
					</rdf:Description>
					<rdf:Description rdf:about="&bfo;BFO_0000029">
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</rdfs:range>
		<skos:example>this transportation process starts at the site of this storage facility</skos:example>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">relationship from a process to a spatial region or site whevever the spatial region o site is the location in which the process starts</iof-av:naturalLanguageDefinition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&iof-constr;suppliesToAtSomeTime">
		<rdfs:label xml:lang="en-US">supplies to at some time</rdfs:label>
		<rdfs:domain rdf:resource="&iof-constr;Supplier"/>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/supplychain/SupplyChain/</rdfs:isDefinedBy>
		<rdfs:range rdf:resource="&iof-constr;Customer"/>
		<owl:propertyChainAxiom rdf:parseType="Collection">
			<rdf:Description rdf:about="&bfo;BFO_0000196">
			</rdf:Description>
			<rdf:Description>
				<owl:inverseOf rdf:resource="&iof-constr;dependsOnSupplier"/>
			</rdf:Description>
			<rdf:Description rdf:about="&iof-constr;dependsOnBuyer">
			</rdf:Description>
			<rdf:Description rdf:about="&bfo;BFO_0000197">
			</rdf:Description>
		</owl:propertyChainAxiom>
		<skos:example>a suppliers of airbag systems suppliers airbags to Honda</skos:example>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">relationship between a supplier and a customer when the supplier sells a product of service to a customer</iof-av:naturalLanguageDefinition>
	</owl:ObjectProperty>
	
	<owl:NamedIndividual rdf:about="&iof-ind;GLNScheme">
