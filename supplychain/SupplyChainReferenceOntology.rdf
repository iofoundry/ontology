		<rdfs:range rdf:resource="&iof-core;Customer"/>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">relation from a supply relationship to a buyer (the bearer), in which the relationship mediates between the buyer and some supplier and some product or service</iof-av:naturalLanguageDefinition>
	</owl:ObjectProperty>

	<owl:ObjectProperty rdf:about="&iof-scro;dependsOnProduct">
		<rdfs:subPropertyOf rdf:resource="&bfo;BFO_0000195"/>
