							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&bfo;BFO_0000199"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&bfo;BFO_0000117"/>
						<owl:someValuesFrom>
							<owl:Restriction>
								<owl:onProperty rdf:resource="&bfo;BFO_0000221"/>
								<owl:someValuesFrom>
									<owl:Restriction>
										<owl:onProperty>
											<rdf:Description>
												<owl:inverseOf rdf:resource="&bfo;BFO_0000199"/>
											</rdf:Description>
										</owl:onProperty>
										<owl:someValuesFrom>
											<owl:Class>
												<owl:unionOf rdf:parseType="Collection">
													<rdf:Description rdf:about="&iof-scro;TransportProcess">
													</rdf:Description>
													<rdf:Description rdf:about="&iof-scro;TravelProcess">
													</rdf:Description>
												</owl:unionOf>
											</owl:Class>
										</owl:someValuesFrom>
									</owl:Restriction>
								</owl:someValuesFrom>
