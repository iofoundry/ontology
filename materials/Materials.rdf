        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
        <rdfs:comment xml:lang="en">role of an object to be consumed in the realization of a process. items which &quot;get used up&quot; or discarded have the role of a consumable</rdfs:comment>
        <rdfs:label xml:lang="en">consumable role</rdfs:label>
    </owl:Class>
    


