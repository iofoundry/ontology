    <!-- https://spec.industrialontologies.org/ontology/materials/Materials/ActOfAnalysing -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/materials/Materials/ActOfAnalysing">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/PlannedProcess"/>
        <rdfs:label xml:lang="de">Analysevorgang</rdfs:label>
        <rdfs:label xml:lang="en">act of analysing</rdfs:label>
        <skos:definition xml:lang="de">Der Prozess des Zerlegens eines komplexen Themas oder Stoffes in kleinere Teile, um ein besseres Verständnis davon zu erlangen.</skos:definition>
        <skos:definition xml:lang="en">process of breaking down a complex topic or substance into smaller parts in order to gain a better understanding of it.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/materials/Materials/ActOfSummarising -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/materials/Materials/ActOfSummarising">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/PlannedProcess"/>
        <rdfs:label xml:lang="de">Zusammenfassungshandlung</rdfs:label>
        <rdfs:label xml:lang="en">act of summarising</rdfs:label>
        <skos:definition xml:lang="en">The act of summarising is the process of taking a large body of information and condensing it into a shorter, more concise version. It involves selecting key points from the original text and presenting them in a new way. The goal of summarising is to provide a clear and accurate overview of the main ideas and arguments of the original text.</skos:definition>
        <iof-av:naturalLanguageDefinition_source rdf:resource="https://en.wikipedia.org/wiki/Summarization"/>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/materials/Materials/AnnealingProcess -->

    <owl:Class rdf:about="https://spec.industrialontologies.org/ontology/materials/Materials/AnnealingProcess">
        <rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/ManufacturingProcess"/>
        <rdfs:label xml:lang="de">Thermische Konditionierung</rdfs:label>
        <rdfs:label xml:lang="en">annealing process</rdfs:label>
        <skos:altLabel xml:lang="de">Wärmebehandlung</skos:altLabel>
        <skos:altLabel xml:lang="en">heat treatment</skos:altLabel>
        <iof-av:naturalLanguageDefinition xml:lang="en">process involving a participant triggering a temperaure change in a object that is input to the process with the scope of creating an effect on the material structure of that object</iof-av:naturalLanguageDefinition>
        <iof-av:naturalLanguageDefinition_source rdf:resource="https://en.wikipedia.org/wiki/Annealing_(metallurgy)"/>
    </owl:Class>
    


