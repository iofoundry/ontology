        <rdfs:label xml:lang="de">Kalibrierzertifikat</rdfs:label>
        <rdfs:label xml:lang="en">calibration certificate</rdfs:label>
        <iof-av:naturalLanguageDefinition xml:lang="en">A certificate verifying a measurment capability of an material artifact.</iof-av:naturalLanguageDefinition>
    </owl:Class>
    


    <!-- https://spec.industrialontologies.org/ontology/materials/Materials/ConsumableRole -->

