        <rdfs:label xml:lang="en">Number Density</rdfs:label>
        <skos:definition xml:lang="en">Number density is a measure of the number of objects in a given unit volume, or unit area.</skos:definition>
        <skos:prefLabel xml:lang="en">Number Density</skos:prefLabel>
        <iof-av:naturalLanguageDefinition_source rdf:resource="https://en.wikipedia.org/wiki/Number_density"/>
