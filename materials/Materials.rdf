        <owl:imports rdf:resource="https://github.com/iofoundry/ontology/raw/master/core/Core.rdf"/>
        <iof-av:usageNote xml:lang="en-us">Use at will</iof-av:usageNote>
        <dcterms:contributor xml:lang="en">Thomas Hanke, Fraunhofer IWM</dcterms:contributor>
        <iof-av:maturity rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Provisional"/>
        <dcterms:references rdf:resource="http://purl.org/dc/terms/"/>
