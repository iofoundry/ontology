        <rdfs:label xml:lang="de">Materialdefekt</rdfs:label>
        <rdfs:label xml:lang="en">material defect</rdfs:label>
        <skos:altLabel xml:lang="de">Materialfehler</skos:altLabel>
        <skos:definition xml:lang="en">material defect is a flaw in a material that can cause it to fail to perform its intended function.</skos:definition>
        <iof-av:naturalLanguageDefinition_source rdf:resource="https://en.wikipedia.org/wiki/Material_defect"/>
    </owl:Class>
    


