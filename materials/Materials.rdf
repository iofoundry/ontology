        <skos:altLabel xml:lang="en">Adhesive Strength</skos:altLabel>
        <skos:altLabel xml:lang="de">Klebkraft</skos:altLabel>
        <skos:definition xml:lang="en">Adhesion strength is the measure of the force required to separate two surfaces that are bonded together.</skos:definition>
        <skos:definition xml:lang="de">Die Haftfestigkeit ist das Maß für die Kraft, die erforderlich ist, um zwei miteinander verbundene Oberflächen zu trennen.</skos:definition>
        <skos:prefLabel xml:lang="en">Adhesion Strength</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Haftfestigkeit</skos:prefLabel>
        <iof-av:naturalLanguageDefinition_source rdf:resource="https://en.wikipedia.org/wiki/Adhesion_strength"/>
    </owl:Class>
    


