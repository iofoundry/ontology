        <rdfs:label xml:lang="en">measurement device</rdfs:label>
        <skos:altLabel xml:lang="de">Messinstrument</skos:altLabel>
        <skos:definition xml:lang="de">Ein Artefakt, das eine Messartefaktfunktion besitzt</skos:definition>
