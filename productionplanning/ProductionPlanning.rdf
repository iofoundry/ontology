	<owl:Class rdf:about="&iof-const;MachineCapability">
		<rdfs:subClassOf rdf:resource="&iof-const;ArtifactCapability"/>
		<rdfs:label xml:lang="en-us">machine capability</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/productionplanning/ProductionPlanning/</rdfs:isDefinedBy>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&iof-const;ArtifactCapability">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&iof-const;capabilityOf"/>
						<owl:someValuesFrom rdf:resource="&iof-const;ProductionMachine"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
		<iof-av:firstOrderLogicAxiom xml:lang="en-us">forall c (MachineCapability(c) = ArtifactCapability(c) and exists m f (MachineElement(m) and MachiningFunction(f) and inheres_in(c,m) and demarcates(c,f)))</iof-av:firstOrderLogicAxiom>
		<iof-av:naturalLanguageDefinition xml:lang="en-us">An Artifact Capability that is borne by a Machine Element and demarcates a Machining Function.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&iof-const;MachineTransferProcess">
		<rdfs:subClassOf rdf:resource="&iof-const;MaterialLocationChangeProcess"/>
		<rdfs:label xml:lang="en-us">machine transfer process</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/productionplanning/ProductionPlanning/</rdfs:isDefinedBy>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&iof-const;MaterialLocationChangeProcess">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&obo;BFO_0000057"/>
						<owl:someValuesFrom>
							<owl:Class>
								<owl:intersectionOf rdf:parseType="Collection">
									<rdf:Description rdf:about="&obo;BFO_0000040">
									</rdf:Description>
									<owl:Restriction>
										<owl:onProperty>
											<rdf:Description>
												<owl:inverseOf rdf:resource="&obo;BFO_0000170"/>
											</rdf:Description>
										</owl:onProperty>
										<owl:someValuesFrom>
											<owl:Class>
												<owl:intersectionOf rdf:parseType="Collection">
													<rdf:Description rdf:about="&iof-const;ProductionMachine">
													</rdf:Description>
													<owl:Restriction>
														<owl:onProperty rdf:resource="&obo;BFO_0000082"/>
														<owl:someValuesFrom rdf:resource="&obo;BFO_0000029"/>
													</owl:Restriction>
													<owl:Restriction>
														<owl:onProperty rdf:resource="&obo;BFO_0000108"/>
														<owl:someValuesFrom>
															<owl:Class>
																<owl:intersectionOf rdf:parseType="Collection">
																	<rdf:Description rdf:about="&obo;BFO_0000203">
																	</rdf:Description>
																	<owl:Class>
																		<owl:intersectionOf rdf:parseType="Collection">
																			<owl:Restriction>
																				<owl:onProperty rdf:resource="&obo;BFO_0000221"/>
																				<owl:someValuesFrom rdf:resource="&obo;BFO_0000008"/>
																			</owl:Restriction>
																			<owl:Restriction>
																				<owl:onProperty>
																					<rdf:Description>
																						<owl:inverseOf rdf:resource="&obo;BFO_0000199"/>
																					</rdf:Description>
																				</owl:onProperty>
																				<owl:someValuesFrom rdf:resource="&iof-const;PlannedProcess"/>
																			</owl:Restriction>
																		</owl:intersectionOf>
																	</owl:Class>
																</owl:intersectionOf>
															</owl:Class>
														</owl:someValuesFrom>
													</owl:Restriction>
												</owl:intersectionOf>
											</owl:Class>
										</owl:someValuesFrom>
									</owl:Restriction>
									<owl:Restriction>
										<owl:onProperty>
											<rdf:Description>
												<owl:inverseOf rdf:resource="&obo;BFO_0000170"/>
											</rdf:Description>
										</owl:onProperty>
										<owl:someValuesFrom>
											<owl:Class>
												<owl:intersectionOf rdf:parseType="Collection">
													<rdf:Description rdf:about="&iof-const;ProductionMachine">
													</rdf:Description>
													<owl:Restriction>
														<owl:onProperty rdf:resource="&obo;BFO_0000082"/>
														<owl:someValuesFrom rdf:resource="&obo;BFO_0000029"/>
													</owl:Restriction>
													<owl:Restriction>
														<owl:onProperty rdf:resource="&obo;BFO_0000108"/>
														<owl:someValuesFrom>
															<owl:Class>
																<owl:intersectionOf rdf:parseType="Collection">
																	<rdf:Description rdf:about="&obo;BFO_0000203">
																	</rdf:Description>
																	<owl:Restriction>
																		<owl:onProperty rdf:resource="&obo;BFO_0000223"/>
																		<owl:someValuesFrom>
																			<owl:Class>
																				<owl:intersectionOf rdf:parseType="Collection">
																					<rdf:Description rdf:about="&obo;BFO_0000008">
																					</rdf:Description>
																					<owl:Restriction>
																						<owl:onProperty>
																							<rdf:Description>
																								<owl:inverseOf rdf:resource="&obo;BFO_0000199"/>
																							</rdf:Description>
