		<rdfs:label xml:lang="en-us">machine transfer process</rdfs:label>
		<rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">https://spec.industrialontologies.org/ontology/productionplanning/ProductionPlanning/</rdfs:isDefinedBy>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&iof-const;MaterialLocationChangeProcess">
					</rdf:Description>
