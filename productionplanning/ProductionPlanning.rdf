	<owl:Class rdf:about="&iof-pps;MachineTransferProcess">
		<rdfs:subClassOf rdf:resource="&iof-core;MaterialLocationChangeProcess"/>
		<rdfs:label xml:lang="en">machine transfer process</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
