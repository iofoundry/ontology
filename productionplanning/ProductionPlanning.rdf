														<owl:onProperty rdf:resource="&obo;BFO_0000108"/>
														<owl:someValuesFrom>
															<owl:Class>
																<owl:intersectionOf rdf:parseType="Collection">
																	<rdf:Description rdf:about="&obo;BFO_0000203">
																	</rdf:Description>
																	<owl:Restriction>
																		<owl:onProperty rdf:resource="&obo;BFO_0000223"/>
