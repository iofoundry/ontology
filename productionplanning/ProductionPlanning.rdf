	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/productionplanning/ProductionPlanning/MachineCapability">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/productionplanning/ProductionPlanning/ArtifactCapability"/>
		<rdfs:label xml:lang="en">machine capability</rdfs:label>
		<AnnotationVocabulary1:firstOrderLogicAxiom xml:lang="en">forall c (MachineCapability(c) = ArtifactCapability(c) and exists m f (MachineElement(m) and MachiningFunction(f) and inheres_in(c,m) and demarcates(c,f)))</AnnotationVocabulary1:firstOrderLogicAxiom>
		<AnnotationVocabulary1:naturalLanguageDefinition xml:lang="en">An Artifact Capability that is borne by a Machine Element and demarcates a Machining Function.</AnnotationVocabulary1:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/productionplanning/ProductionPlanning/MachineTransferProcess">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/MaterialLocationChangeProcess"/>
		<rdfs:label xml:lang="en">machine transfer process</rdfs:label>
