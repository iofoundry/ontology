																				<owl:someValuesFrom rdf:resource="&obo;BFO_0000008"/>
																			</owl:Restriction>
																			<owl:Restriction>
																				<owl:onProperty>
																					<rdf:Description>
																						<owl:inverseOf rdf:resource="&obo;BFO_0000199"/>
																					</rdf:Description>
																				</owl:onProperty>
																				<owl:someValuesFrom rdf:resource="&iof-const;PlannedProcess"/>
																			</owl:Restriction>
																		</owl:intersectionOf>
																	</owl:Class>
