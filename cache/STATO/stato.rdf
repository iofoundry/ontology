from:
https://www.rdocumentation.org/packages/metRology/versions/0.9-23-2/topics/Scaled%20t%20distribution</obo:STATO_0000041>
	</owl:Class>
	
	<owl:Class rdf:about="&obo;STATO_0000530">
		<rdfs:subClassOf rdf:resource="&obo;STATO_0000107"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&obo;STATO_0000001"/>
