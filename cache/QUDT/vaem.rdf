		<vaem:namespacePrefix>vaem</vaem:namespacePrefix>
		<vaem:owner>TopQuadrant, Inc.</vaem:owner>
		<vaem:previousPublishedVersion rdf:datatype="&xsd;anyURI">http://linkedmodel.org/doc/vaem/1.2/</vaem:previousPublishedVersion>
		<vaem:rdfxmlFileURL rdf:datatype="&xsd;anyURI">http://www.linkedmodel.org/2.0/schema/SCHEMA_vaem-v2.0.rdf</vaem:rdfxmlFileURL>
		<vaem:revision>2.0</vaem:revision>
