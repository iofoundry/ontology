		<rdfs:isDefinedBy rdf:resource="http://www.linkedmodel.org/schema/vaem"/>
	</vaem:GraphRole>
	
	<vaem:GraphRole rdf:about="&vaem;ConstraintsGraph">
		<rdfs:label>Constraints graph</rdfs:label>
