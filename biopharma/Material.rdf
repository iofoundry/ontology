                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000115"/>
                        <owl:someValuesFrom rdf:resource="https://spec.industrialontologies.org/ontology/construct/Cell"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000115"/>
                        <owl:allValuesFrom rdf:resource="https://spec.industrialontologies.org/ontology/construct/Cell"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
