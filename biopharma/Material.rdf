			</owl:Class>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&bfo;BFO_0000054"/>
				<owl:allValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&bfo;BFO_0000132"/>
						<owl:someValuesFrom rdf:resource="&iof-core;ProductProductionProcess"/>
					</owl:Restriction>
				</owl:allValuesFrom>
			</owl:Restriction>
