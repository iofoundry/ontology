										<owl:someValuesFrom>
											<owl:Class>
												<owl:intersectionOf rdf:parseType="Collection">
													<rdf:Description rdf:about="&iof-core;Capability">
													</rdf:Description>
													<owl:Restriction>
														<owl:onProperty rdf:resource="&bfo;BFO_0000054"/>
														<owl:someValuesFrom>
															<owl:Restriction>
																<owl:onProperty rdf:resource="&bfo;BFO_0000132"/>
																<owl:someValuesFrom rdf:resource="&iof-core;ProductProductionProcess"/>
															</owl:Restriction>
														</owl:someValuesFrom>
													</owl:Restriction>
												</owl:intersectionOf>
											</owl:Class>
