<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY bfo "http://purl.obolibrary.org/obo/">
	<!ENTITY bmic-equipment "https://spec.industrialontologies.org/ontology/biopharma/Equipment/">
	<!ENTITY dc "http://purl.org/dc/elements/1.1/">
	<!ENTITY dcterms "http://purl.org/dc/terms/">
	<!ENTITY dtype "http://www.linkedmodel.org/schema/dtype#">
	<!ENTITY iof-av "https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/">
	<!ENTITY iof-core "https://spec.industrialontologies.org/ontology/core/Core/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY qudt "http://qudt.org/schema/qudt/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY terms "http://purl.org/dc/terms/">
	<!ENTITY vaem "http://www.linkedmodel.org/schema/vaem#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.industrialontologies.org/ontology/biopharma/Equipment/"
	xmlns:bfo="http://purl.obolibrary.org/obo/"
	xmlns:bmic-equipment="https://spec.industrialontologies.org/ontology/biopharma/Equipment/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dtype="http://www.linkedmodel.org/schema/dtype#"
	xmlns:iof-av="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/"
	xmlns:iof-core="https://spec.industrialontologies.org/ontology/core/Core/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:qudt="http://qudt.org/schema/qudt/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:terms="http://purl.org/dc/terms/"
	xmlns:vaem="http://www.linkedmodel.org/schema/vaem#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.industrialontologies.org/ontology/biopharma/Equipment/">
		<rdfs:label xml:lang="en-US">Equipment Ontology</rdfs:label>
		<terms:abstract>This module contains terms and relations enabling representation of equipment types, properties (qualities, capabilities, and functions), equipment specifications and validation (e.g., methods, testing, reporting, etc.).</terms:abstract>
		<terms:contributor>Adlane Rebai, Millipore Sigma</terms:contributor>
		<terms:contributor>Cameron Gibbs, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Melissa Weller, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Milos Drobnjakovic, National Institute of Standards and Technology (NIST)</terms:contributor>
		<terms:contributor>Stephen J. Granite, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Stephen Kahmann, CrownPoint Technologies, LLC</terms:contributor>
		<terms:creator>NIIMBL BD-1 Ontology Group</terms:creator>
		<dcterms:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dcterms:license>
		<owl:imports rdf:resource="http://qudt.org/2.1/schema/qudt/"/>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/"/>
		<owl:versionIRI rdf:resource="https://spec.industrialontologies.org/ontology/202401/biopharma/Equipment/"/>
		<iof-av:copyright>Copyright (c) 2022, 2023, Open Applications Group</iof-av:copyright>
		<iof-av:maturity rdf:resource="&iof-av;Provisional"/>		
	</owl:Ontology>
	
	<owl:Class rdf:about="&bmic-equipment;AffinityChromatographySystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">affinity chromatography system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>http://wolfson.huji.ac.il/purification/PDF/affinity/GE_Affinity_Chromatography_Handbook-Antibodies.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">chromatography system based on various types of specific affinities between target molecule(s), for example, a protein and a specific ligand coupled to a chromatography medium.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;AnalyticChromatographicSystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">analytic chromatographic system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>https://www.emdmillipore.com/US/en/products/analytics-sample-prep/chromatography-for-analysis/4aOb.qB.GMkAAAE_NeF3.Lxi,nav</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>chromatographic analysis is also essential in the production of pharmaceuticals, chemicals or food and beverages.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">chromatography system routinely used in industry and academia for separation, quantitation and identification of chemical or biological compounds</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;AnionicExchangeChromatographySystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">anionic exchange chromatography system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>http://wolfson.huji.ac.il/purification/PDF/affinity/GE_Affinity_Chromatography_Handbook-Antibodies.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">chromatography system based on negatively-charged interactions between solutes (for example, proteins) and chromatography medium.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;Bioreactor">
		<rdfs:subClassOf rdf:resource="&iof-core;EngineeredSystem"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;designatedBy"/>
				<owl:someValuesFrom rdf:resource="&bmic-equipment;EngineeredSystemIdentification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">bioreactor</rdfs:label>
		<iof-av:adaptedFrom>https://www.sciencedirect.com/topics/earth-and-planetary-sciences/bioreactor</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">engineered system deployed to facilitate the growth of biological mass through the transformation or degradation of material fed to the reactor.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;CationicExchangeChromatographySystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">cationic exchange chromatography system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>http://wolfson.huji.ac.il/purification/PDF/affinity/GE_Affinity_Chromatography_Handbook-Antibodies.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">chromatography system based on positively-charged interactions between solutes (for example, proteins) and chromatography medium.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;ChromatographySystem">
		<rdfs:subClassOf rdf:resource="&iof-core;EngineeredSystem"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;designatedBy"/>
				<owl:someValuesFrom rdf:resource="&bmic-equipment;EngineeredSystemIdentification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">chromatography system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>http://wolfson.huji.ac.il/purification/PDF/affinity/GE_Affinity_Chromatography_Handbook-Antibodies.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">engineered system that produces a graphical presentation of detector response(s) indicating the concentration of the solutes coming out of the column during the purification (volume or time).</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;DepthFilter">
		<rdfs:subClassOf rdf:resource="&iof-core;EngineeredSystem"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;designatedBy"/>
				<owl:someValuesFrom rdf:resource="&bmic-equipment;EngineeredSystemIdentification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">depth filter</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>https://www.cytivalifesciences.com/en/us/solutions/lab-filtration/knowledge-center/glossary/depth-filter</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>particles are entrapped or adsorbed both within and on the filter due to a random matrix or structure that creates a tortuous path through the filter.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">filter that does not have a defined pore size or structure.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;EngineeredSystemIdentification">
		<rdfs:subClassOf rdf:resource="&iof-core;Identifier"/>
		<rdfs:label xml:lang="en-US">engineered system identification</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&iof-core;Identifier">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&iof-core;designates"/>
						<owl:someValuesFrom rdf:resource="&iof-core;EngineeredSystem"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">identifier for a system that is deliberately created to have a certain function</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;HydrophobicChromatographySystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">hydrophobic chromatography system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">chromatography system based on the hydrophobic interaction between solutes (for example, proteins) and the chromatography medium in the presence of high salt concentration.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;MixedModeChromatographySystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">mixed mode chromatography system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>http://wolfson.huji.ac.il/purification/PDF/HCIC/GE_MultimodalChromat.pdf</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>different modes of interaction can be expected depending on experimental conditions. These interactions can cooperate or work independently.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">chromatography system with separation through at least one ligand type that has more than one interaction site.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;ProcessChromatographicSystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">process chromatographic system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>https://www.cytivalifesciences.com/en/us/shop/chromatography/chromatography-systems/aktaprocess-system-p-06167</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>proven design has been verified during development and can be user configured to meet specific process demands.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">automated liquid chromatography system built for process scale-up and large-scale biopharmaceutical manufacturing.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;ProteinCaptureColumn">
		<rdfs:subClassOf rdf:resource="&iof-core;EngineeredSystem"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;designatedBy"/>
				<owl:someValuesFrom rdf:resource="&bmic-equipment;EngineeredSystemIdentification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">protein capture column</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<skos:altLabel xml:lang="en">affinity chromatography column</skos:altLabel>
		<iof-av:adaptedFrom>http://wolfson.huji.ac.il/purification/PDF/affinity/GE_Affinity_Chromatography_Handbook-Antibodies.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">engineered system based on various types of specific affinities between target molecule(s), for example, a protein and a specific ligand coupled to a chromatography medium.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;ReversePhaseChromatographySystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">reverse phase chromatography system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:adaptedFrom>http://wolfson.huji.ac.il/purification/PDF/affinity/GE_Affinity_Chromatography_Handbook-Antibodies.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">chromatography system with hydrophobic interactions between solutes (sample components) and ligands coupled to the chromatography medium.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;SizeExclusionChromatographySystem">
		<rdfs:subClassOf rdf:resource="&bmic-equipment;ChromatographySystem"/>
		<rdfs:label xml:lang="en-US">size exclusion chromatography system</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<skos:altLabel>gel filtration</skos:altLabel>
		<iof-av:adaptedFrom>http://wolfson.huji.ac.il/purification/PDF/affinity/GE_Affinity_Chromatography_Handbook-Antibodies.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">chromatography system that separates solutes (for example, proteins) according to size.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-equipment;ThawingSystem">
		<rdfs:subClassOf rdf:resource="&iof-core;EngineeredSystem"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;designatedBy"/>
				<owl:someValuesFrom rdf:resource="&bmic-equipment;EngineeredSystemIdentification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">thawing system</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">engineered system intended to raise the temperature of a container to a certain level</iof-av:naturalLanguageDefinition>
	</owl:Class>

</rdf:RDF>
