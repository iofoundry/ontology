		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;capabilityOf"/>
				<owl:allValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&iof-core;Agent">
							</rdf:Description>
							<rdf:Description rdf:about="&iof-core;PieceOfEquipment">
							</rdf:Description>
						</owl:unionOf>
