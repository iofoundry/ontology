					<owl:Restriction>
						<owl:onProperty rdf:resource="&iof-core;prescribes"/>
						<owl:someValuesFrom>
							<owl:Class>
								<owl:intersectionOf rdf:parseType="Collection">
									<owl:Class>
										<owl:unionOf rdf:parseType="Collection">
											<rdf:Description rdf:about="&bfo;BFO_0000019">
