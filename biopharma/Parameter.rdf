<!DOCTYPE rdf:RDF [
	<!ENTITY bfo "http://purl.obolibrary.org/obo/">
	<!ENTITY bmic-parameter "https://spec.industrialontologies.org/ontology/parameter/">
	<!ENTITY dc "http://purl.org/dc/elements/1.1/">
	<!ENTITY dcterms "http://purl.org/dc/terms/">
	<!ENTITY dtype "http://www.linkedmodel.org/schema/dtype#">
	<!ENTITY iof-av "https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/">
	<!ENTITY iof-core "https://spec.industrialontologies.org/ontology/core/Core/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY qudt "http://qudt.org/schema/qudt/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY terms "http://purl.org/dc/terms/">
	<!ENTITY vaem "http://www.linkedmodel.org/schema/vaem#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.industrialontologies.org/ontology/parameter/"
	xmlns:bfo="http://purl.obolibrary.org/obo/"
	xmlns:bmic-parameter="https://spec.industrialontologies.org/ontology/parameter/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dtype="http://www.linkedmodel.org/schema/dtype#"
	xmlns:iof-av="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/"
	xmlns:iof-core="https://spec.industrialontologies.org/ontology/core/Core/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:qudt="http://qudt.org/schema/qudt/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:terms="http://purl.org/dc/terms/"
	xmlns:vaem="http://www.linkedmodel.org/schema/vaem#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.industrialontologies.org/ontology/parameter/">
		<rdfs:label xml:lang="en-US">Parameter Ontology</rdfs:label>
		<terms:abstract>This module contains terms and relations necessary for representing  process intermediate and product quality attributes, process indicators and process parameters.</terms:abstract>
		<terms:contributor>Adlane Rebai, Millipore Sigma</terms:contributor>
		<terms:contributor>Cameron Gibbs, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Melissa Weller, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Milos Drobnjakovic, National Institute of Standards and Technology (NIST)</terms:contributor>
		<terms:contributor>Stephen J. Granite, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Stephen Kahmann, CrownPoint Technologies, LLC</terms:contributor>
		<terms:creator>NIIMBL BD-1 Ontology Group</terms:creator>
		<dcterms:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dcterms:license>
		<owl:imports rdf:resource="http://qudt.org/2.1/schema/qudt"/>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/"/>
		<owl:versionIRI rdf:resource="https://spec.industrialontologies.org/ontology/202401/parameter/"/>
		<iof-av:copyright>Copyright (c) 2022, 2023, Open Applications Group</iof-av:copyright>
		<iof-av:maturity rdf:resource="&iof-av;Provisional"/>			
	</owl:Ontology>
	
	<owl:Class rdf:about="&bmic-parameter;CriticalProcessParameterSpecification">
		<rdfs:subClassOf rdf:resource="&bmic-parameter;ProcessParameterSpecification"/>
		<rdfs:label xml:lang="en-US">critical process parameter specification</rdfs:label>
		<iof-av:adaptedFrom>https://database.ich.org/sites/default/files/Q8%28R2%29%20Guideline.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">process parameter specification whose variability has an impact on a critical quality attribute and, therefore, should be monitored or controlled to ensure the process obtains the desired quality</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;CriticalQualityAttributeSpecification">
		<rdfs:subClassOf rdf:resource="&bmic-parameter;QualityAttributeSpecification"/>
		<rdfs:label xml:lang="en-US">critical quality attribute specification</rdfs:label>
		<terms:source>https://database.ich.org/sites/default/files/Q8%28R2%29%20Guideline.pdf</terms:source>
		<iof-av:adaptedFrom>https://database.ich.org/sites/default/files/Q8%28R2%29%20Guideline.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">quality attribute specification that is a physical, chemical, biological or microbiological property or characteristic that should be within an appropriate limit, range, or distribution to ensure the desired product quality</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;CriticalRole">
		<rdfs:subClassOf rdf:resource="&bfo;BFO_0000023"/>
		<rdfs:label xml:lang="en-US">critical role</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">role for something that is vital to what is occurring</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:NamedIndividual rdf:about="&bmic-parameter;HighMannose">
		<rdf:type rdf:resource="&bmic-parameter;ParameterKind"/>
		<rdfs:label xml:lang="en-US">high mannose</rdfs:label>
		<terms:contributor>Merck KGaA</terms:contributor>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">High-mannose glycans contain unsubstituted terminal mannose sugars. Glycan moieties attached to the proteins can directly affect protein stability, bioactivity, and immunogenicity.</iof-av:naturalLanguageDefinition>
	</owl:NamedIndividual>
	
	<owl:NamedIndividual rdf:about="&bmic-parameter;HighMolecularWeight">
		<rdf:type rdf:resource="&bmic-parameter;ParameterKind"/>
		<rdfs:label xml:lang="en-US">high molecular weight</rdfs:label>
		<terms:contributor>Merck KGaA</terms:contributor>
		<iof-av:abbreviation>HMW</iof-av:abbreviation>
		<iof-av:adaptedFrom>https://www.industrialchemicals.gov.au/glossary/high-molecular-weight-polymer</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">A polymer that has a number average molecular weight that is greater than or equal to 1,000 g/mol. HMW species within a therapeutic mAb drug product as a result of protein aggregation can potentially compromise both drug efficacy and safety.</iof-av:naturalLanguageDefinition>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&bmic-parameter;KeyPerformanceIndicatorSpecification">
		<rdfs:subClassOf rdf:resource="&bmic-parameter;PerformanceIndicatorSpecification"/>
		<rdfs:label xml:lang="en-US">key performance indicator specification</rdfs:label>
		<skos:altLabel xml:lang="en">key process indicator</skos:altLabel>
		<skos:altLabel xml:lang="en">process performance indicator</skos:altLabel>
		<iof-av:adaptedFrom>White Paper: Biopharma PATQuality Attributes, Critical Process Parameters &amp; Key Performance Indicators at the Bioreactor</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>kpis are related to CQAs and therefore influenced, as well, by the CPPs. As the CPPs remain within the pre-defined limits, the KPIs should indicate that each production step proceeds accordingly resulting, in the end, in a product having its CQAs within the appropriate limits, too.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">metric for the status of each production step</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;KeyProcessParameterSpecification">
		<rdfs:subClassOf rdf:resource="&bmic-parameter;ProcessParameterSpecification"/>
		<rdfs:label xml:lang="en-US">key process parameter specification</rdfs:label>
		<iof-av:adaptedFrom>https://www.biophorum.com/wp-content/uploads/bp_downloads/CPV-Case-Study-Interactive-Version.pdf</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>key process parameter does not meaningfully affect critical product quality attributes.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">adjustable process parameter (variable) specification that, when maintained within a narrow range, ensures optimum process performance</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;ParameterKind">
		<rdfs:subClassOf rdf:resource="&iof-core;DescriptiveInformationContentEntity"/>
		<rdfs:label xml:lang="en-US">parameter kind</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">descriptive information content entity to denote the kind of parameter being described</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;ParameterObjectiveSpecification">
		<rdfs:subClassOf rdf:resource="&iof-core;ObjectiveSpecification"/>
		<rdfs:label xml:lang="en-US">parameter objective specification</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">objective specification for a parameter in an ISA-88 recipe component</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;ParameterRequirementSpecification">
		<rdfs:subClassOf rdf:resource="&iof-core;RequirementSpecification"/>
		<rdfs:label xml:lang="en-US">parameter requirement specification</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">requirement specification which specifies an information object that has the contextual role of a parameter in the realized process</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;PerformanceIndicatorSpecification">
		<rdfs:subClassOf rdf:resource="&bmic-parameter;ParameterObjectiveSpecification"/>
		<rdfs:label xml:lang="en-US">performance indicator specification</rdfs:label>
		<skos:altLabel>process indicator</skos:altLabel>
		<iof-av:adaptedFrom>https://database.ich.org/sites/default/files/Q10%20Guideline.pdf</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>Performance indicator specification is also known as “performance metrics” in some regions.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">measurable values used to quantify quality objectives to reflect the performance of an organization, process or system</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;ProcessParameterSpecification">
		<rdfs:subClassOf rdf:resource="&bmic-parameter;ParameterObjectiveSpecification"/>
		<rdfs:label xml:lang="en-US">process parameter specification</rdfs:label>
		<skos:altLabel xml:lang="en">parameter</skos:altLabel>
		<skos:altLabel xml:lang="en">process value</skos:altLabel>
		<skos:altLabel xml:lang="en">process variable</skos:altLabel>
		<iof-av:adaptedFrom>https://www.biophorum.com/wp-content/uploads/bp_downloads/CPV-Case-Study-Interactive-Version.pdf</iof-av:adaptedFrom>
		<iof-av:explanatoryNote xml:lang="en">1) The variable may or may not have a critical effect on product quality or process performance.
    2) Ranges for GPPs are established during process development, and changes to operating ranges will be managed within the quality system.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter objective specification that specifies (prescribes) the range of values that an adjustable parameter (variable) of the process should have</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;QualityAttributeSpecification">
		<rdfs:subClassOf rdf:resource="&bmic-parameter;ParameterObjectiveSpecification"/>
		<rdfs:label xml:lang="en-US">quality attribute specification</rdfs:label>
		<skos:altLabel xml:lang="en">non-functional requirement</skos:altLabel>
		<iof-av:adaptedFrom>https://www.biophorum.com/wp-content/uploads/bp_downloads/CPV-Case-Study-Interactive-Version.pdf</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>collectively, the quality attributes define the purity, potency, identity, and stability of the product.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter objective specification of a molecular or product characteristic that is selected for its ability to help indicate the quality of the product</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-parameter;SystemParameterSpecification">
		<rdfs:subClassOf rdf:resource="&bmic-parameter;ParameterRequirementSpecification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;prescribes"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&iof-core;System">
							</rdf:Description>
							<owl:Class>
								<owl:intersectionOf rdf:parseType="Collection">
									<rdf:Description rdf:about="&bfo;BFO_0000020">
									</rdf:Description>
									<owl:Restriction>
										<owl:onProperty rdf:resource="&bfo;BFO_0000195"/>
										<owl:someValuesFrom rdf:resource="&iof-core;System"/>
									</owl:Restriction>
								</owl:intersectionOf>
							</owl:Class>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">system parameter specification</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter requirement specification that prescribes a system or an attribute of the system and that has quantity whose value influences the behavior of the system or the attribute</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&bmic-parameter;hasNormalOperatingRange">
		<rdfs:subPropertyOf rdf:resource="&iof-core;isAbout"/>
		<rdfs:label xml:lang="en-US">has normal operating range</rdfs:label>
		<rdfs:domain rdf:resource="&bmic-parameter;ProcessParameterSpecification"/>
		<rdfs:range rdf:resource="&qudt;Quantity"/>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">defined range, within the proven acceptable range, specified in the manufacturing instructions as the target and range at which a process parameter is controlled, while producing unit operation material or final product meeting release criteria and critical quality attributes</iof-av:naturalLanguageDefinition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&bmic-parameter;hasParameterKind">
		<rdfs:subPropertyOf rdf:resource="&iof-core;isAbout"/>
		<rdfs:label xml:lang="en-US">has parameter kind</rdfs:label>
		<rdfs:domain rdf:resource="&bmic-parameter;ParameterRequirementSpecification"/>
		<rdfs:range rdf:resource="&bmic-parameter;ParameterKind"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&bmic-parameter;hasProvenAcceptableRange">
		<rdfs:subPropertyOf rdf:resource="&iof-core;isAbout"/>
		<rdfs:label xml:lang="en-US">has proven acceptable range</rdfs:label>
		<rdfs:domain rdf:resource="&bmic-parameter;ProcessParameterSpecification"/>
		<rdfs:range rdf:resource="&qudt;Quantity"/>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">characterized range of a process parameter for which operation within this range, while keeping other parameters constant, will result in producing a material meeting relevant quality criteria</iof-av:naturalLanguageDefinition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&bmic-parameter;hasSetpoint">
		<rdfs:subPropertyOf rdf:resource="&iof-core;isAbout"/>
		<rdfs:label xml:lang="en-US">has set point</rdfs:label>
		<rdfs:domain rdf:resource="&bmic-parameter;ProcessParameterSpecification"/>
		<rdfs:range rdf:resource="&qudt;Quantity"/>
	</owl:ObjectProperty>

</rdf:RDF>