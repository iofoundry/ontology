		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en-US">filtration process in which a fluid is passed through a depth filter</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-recipe;DiafiltrationProcess">
		<rdfs:subClassOf rdf:resource="&bmic-recipe;MaterialSeparation"/>
		<rdfs:subClassOf>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Restriction>
						<owl:onProperty rdf:resource="&bfo;BFO_0000117"/>
						<owl:someValuesFrom>
							<owl:Class>
								<owl:intersectionOf rdf:parseType="Collection">
									<rdf:Description rdf:about="&bmic-recipe;DilutionProcess">
									</rdf:Description>
									<owl:Restriction>
										<owl:onProperty rdf:resource="&iof-core;before"/>
										<owl:someValuesFrom rdf:resource="&bmic-recipe;FiltrationProcess"/>
									</owl:Restriction>
								</owl:intersectionOf>
							</owl:Class>
						</owl:someValuesFrom>
					</owl:Restriction>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&bfo;BFO_0000117"/>
						<owl:someValuesFrom>
							<owl:Class>
								<owl:intersectionOf rdf:parseType="Collection">
									<rdf:Description rdf:about="&bmic-recipe;DilutionProcess">
									</rdf:Description>
									<owl:Restriction>
										<owl:onProperty rdf:resource="&iof-core;occursSimultaneouslyWith"/>
