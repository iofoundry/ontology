	<owl:Class rdf:about="&iof-core;MaterialStateDescription">
		<rdfs:subClassOf rdf:resource="&iof-core;InformationContentEntity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&bfo;BFO_0000178"/>
				<owl:allValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&iof-core;PhysicalLocationIdentifier">
							</rdf:Description>
							<rdf:Description rdf:about="&iof-core;ProcessCharacteristicDescription">
							</rdf:Description>
							<rdf:Description rdf:about="&iof-core;ValueExpression">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:allValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label rdf:datatype="&rdf;langString" xml:lang="en-US">material state description</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&iof-core;InformationContentEntity">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&iof-core;describes"/>
						<owl:someValuesFrom rdf:resource="&iof-core;MaterialState"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
		<iof-av:firstOrderLogicAxiom rdf:datatype="&xsd;string">LA1: MaterialStateDescription(x) ∧ hasContinuantPartAtSomeTime(x,y)→ (ProcessCharacteristicDescription(y) ∨ ValueExpression(y) ∨ PhysicalLocationIdentifier(y))</iof-av:firstOrderLogicAxiom>
		<iof-av:firstOrderLogicAxiom rdf:datatype="&xsd;string">LA2: MaterialStateDescription(x) ∧ hasContinuantPartAtSomeTime(x,y) ∧ PhysicalLocationIdentifier(y) ∧  MaterialState(s) ∧ describes(x,s) → ∃m ∃t ∃z(MaterialEntity(m) ∧ hasMaterialState(m,s) ∧ (TemporalInterval(t) ∨ TemporalInstant(t)) ∧ occupiesTemporalRegion(s,t) ∧ locationOfAtSomeTime(z,m,t) ∧ designates(y,z))</iof-av:firstOrderLogicAxiom>
		<iof-av:firstOrderLogicAxiom rdf:datatype="&xsd;string">LA3: MaterialStateDescription(x) ∧ hasContinuantPartAtSomeTime(x,y) ∧ ProcessCharacteristicDescription(y) ∧ MaterialState(s) ∧ describes(x,s)→ ∃m ∃t ∃c ∃p(MaterialState(s) ∧ describes(x,s) ∧ MaterialEntity(m) ∧ hasMaterialState(m,s) ∧ TemporalInterval(t) ∧ occupiesTemporalRegion(s,t) ∧  ProcessCharacteristic(c) ∧ Process(p) ∧ processCharacteristicOf(c,p) ∧ hasParticipantAtAllTimes(p,m) ∧ describes(y,c) ∧ occupiesTemporalRegion(p,t))</iof-av:firstOrderLogicAxiom>
		<iof-av:firstOrderLogicAxiom rdf:datatype="&xsd;string">LA4: MaterialStateDescription(x) ∧ hasContinuantPartAtSomeTime(x,y) ∧ ValueExpression(y) ∧ MaterialState(s) ∧ describes(x,s) → (∃m ∃t ∃g(MaterialEntity(m) ∧ hasMaterialState(m,s) ∧ (TemporalInterval(t) ∨ TemporalInstant(t)) ∧ occupiesTemporalRegion(s,t) ∧ SpecificallyDependentContinuant(g) ∧  isValueExpressionOfAtSomeTime(y,g,t) ∧ specificallyDependsOn(g,m))
∨ ProcessCharacteristicDescription(y))</iof-av:firstOrderLogicAxiom>
		<iof-av:firstOrderLogicDefinition rdf:datatype="&xsd;string">MaterialStateDescription(x) ↔ InformationContentEntity(x) ∧ ∃s(MaterialState(s) ∧ describes(x,s))</iof-av:firstOrderLogicDefinition>
		<iof-av:naturalLanguageDefinition rdf:datatype="&rdf;langString" xml:lang="en-US">information content entity that characterizes a state</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
