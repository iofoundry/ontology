<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY bfo "http://purl.obolibrary.org/obo/">
	<!ENTITY bmic-material "https://spec.industrialontologies.org/ontology/material/">
	<!ENTITY bmic-monitoringandcontrol "https://spec.industrialontologies.org/ontology/monitoringandcontrol/">
	<!ENTITY bmic-parameter "https://spec.industrialontologies.org/ontology/parameter/">
	<!ENTITY dc "http://purl.org/dc/elements/1.1/">
	<!ENTITY dcterms "http://purl.org/dc/terms/">
	<!ENTITY dtype "http://www.linkedmodel.org/schema/dtype#">
	<!ENTITY iof-av "https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/">
	<!ENTITY iof-core "https://spec.industrialontologies.org/ontology/core/Core/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY qudt "http://qudt.org/schema/qudt/">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY terms "http://purl.org/dc/terms/">
	<!ENTITY vaem "http://www.linkedmodel.org/schema/vaem#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.industrialontologies.org/ontology/monitoringandcontrol/"
	xmlns:bfo="http://purl.obolibrary.org/obo/"
	xmlns:bmic-material="https://spec.industrialontologies.org/ontology/material/"
	xmlns:bmic-monitoringandcontrol="https://spec.industrialontologies.org/ontology/monitoringandcontrol/"
	xmlns:bmic-parameter="https://spec.industrialontologies.org/ontology/parameter/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:dtype="http://www.linkedmodel.org/schema/dtype#"
	xmlns:iof-av="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/"
	xmlns:iof-core="https://spec.industrialontologies.org/ontology/core/Core/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:qudt="http://qudt.org/schema/qudt/"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:terms="http://purl.org/dc/terms/"
	xmlns:vaem="http://www.linkedmodel.org/schema/vaem#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.industrialontologies.org/ontology/monitoringandcontrol/">
		<rdfs:label xml:lang="en-US">Monitoring And Control Ontology</rdfs:label>
		<terms:abstract>This module contains terms and relations necessary for representing the planning and execution of monitoring and/or controlling for both laboratory and manufacturing scale.</terms:abstract>
		<terms:contributor>Adlane Rebai, Millipore Sigma</terms:contributor>
		<terms:contributor>Cameron Gibbs, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Melissa Weller, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Milos Drobnjakovic, National Institute of Standards and Technology (NIST)</terms:contributor>
		<terms:contributor>Stephen J. Granite, CrownPoint Technologies, LLC</terms:contributor>
		<terms:contributor>Stephen Kahmann, CrownPoint Technologies, LLC</terms:contributor>
		<terms:creator>NIIMBL BD-1 Ontology Group</terms:creator>
		<dcterms:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dcterms:license>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/material/"/>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/parameter/"/>
		<owl:versionIRI rdf:resource="https://spec.industrialontologies.org/ontology/202401/monitoringandcontrol/"/>
		<iof-av:copyright>Copyright (c) 2022, 2023, Open Applications Group</iof-av:copyright>
		<iof-av:maturity rdf:resource="&iof-av;Provisional"/>			
	</owl:Ontology>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;ControlStrategy">
		<rdfs:subClassOf rdf:resource="&iof-core;PlanSpecification"/>
		<rdfs:label xml:lang="en-US">control strategy</rdfs:label>
		<skos:altLabel>integrated control strategy</skos:altLabel>
		<skos:altLabel>process control strategy</skos:altLabel>
		<iof-av:adaptedFrom>https://database.ich.org/sites/default/files/Q10%20Guideline.pdf</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>controls can include parameters and attributes related to drug substance and drug product materials and components, facility and equipment operating conditions, in-process controls, finished product specifications, and the associated methods and frequency of monitoring and control.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">plan specification of controls, derived from current product and process understanding, that assures process performance and product quality</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;MachineParameterConfiguration">
		<rdfs:subClassOf rdf:resource="&bmic-monitoringandcontrol;ParameterConfiguration"/>
		<rdfs:label xml:lang="en-US">machine parameter configuration</rdfs:label>
		<iof-av:adaptedFrom>http://purl.allotrope.org/ontologies/quality#AFQ_0000036</iof-av:adaptedFrom>
		<iof-av:adaptedFrom>http://purl.allotrope.org/voc/afo/REC/2022/03/afo</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter configuration of a machine that concretizes some machine setting</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;MonitoringPlan">
		<rdfs:subClassOf rdf:resource="&iof-core;ActionSpecification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;prescribes"/>
				<owl:someValuesFrom rdf:resource="&bmic-monitoringandcontrol;MonitoringProcess"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">monitoring plan</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">action specification to describe all monitoring performed for a control strategy</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;MonitoringProcess">
		<rdfs:subClassOf rdf:resource="&iof-core;PlannedProcess"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&bfo;BFO_0000117"/>
				<owl:someValuesFrom rdf:resource="&iof-core;MeasurementProcess"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">monitoring process</rdfs:label>
		<skos:altLabel>process monitoring</skos:altLabel>
		<iof-av:adaptedFrom>https://www.biopharminternational.com/view/monitoring-biopharmaceutical-processes-present-and-future-approaches</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">planned process for collecting process data and statistical evaluation of parameters to verify and demonstrate that the process is operating in a state of control, identify possible process changes and shifts, and promote continuous improvement</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;ParameterConfiguration">
		<rdfs:subClassOf rdf:resource="&bfo;BFO_0000017"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&bfo;BFO_0000164"/>
				<owl:someValuesFrom rdf:resource="&bmic-parameter;ParameterRequirementSpecification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">parameter configuration</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">realizable entity of a parameter that concretizes some parameter setting</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;ParameterMeasurement">
		<rdfs:subClassOf rdf:resource="&iof-core;MeasurementInformationContentEntity"/>
		<rdfs:label xml:lang="en-US">parameter measurement</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">measurement information content entity for a specific kind of parameter</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;PerformanceIndicatorConfiguration">
		<rdfs:subClassOf rdf:resource="&bmic-monitoringandcontrol;ParameterConfiguration"/>
		<rdfs:label xml:lang="en-US">performance indicator configuration</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter configuration of a performance indicator that concretizes some performance indicator setting</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;PerformanceIndicatorMeasurement">
		<rdfs:subClassOf rdf:resource="&bmic-monitoringandcontrol;ParameterMeasurement"/>
		<rdfs:label xml:lang="en-US">performance indicator measurement</rdfs:label>
		<iof-av:adaptedFrom>https://database.ich.org/sites/default/files/Q10%20Guideline.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">measurable values used to quantify quality objectives to reflect the performance of an organisation, process or system, also known as â€•performance metrics in some regions.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;ProcessParameterConfiguration">
		<rdfs:subClassOf rdf:resource="&bmic-monitoringandcontrol;ParameterConfiguration"/>
		<rdfs:label xml:lang="en-US">process parameter configuration</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter configuration of a process parameter that concretizes some process parameter setting</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;ProcessParameterMeasurement">
		<rdfs:subClassOf rdf:resource="&bmic-monitoringandcontrol;ParameterMeasurement"/>
		<rdfs:label xml:lang="en-US">process parameter measurement</rdfs:label>
		<iof-av:adaptedFrom>https://database.ich.org/sites/default/files/Q10%20Guideline.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter measurement of the range of values that an adjustable parameter (variable) of the process that may or may not have a critical effect on product quality or process performance</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;QualityAttributeConfiguration">
		<rdfs:subClassOf rdf:resource="&bmic-monitoringandcontrol;ParameterConfiguration"/>
		<rdfs:label xml:lang="en-US">quality attribute configuration</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter configuration of a quality attribute that concretizes some quality attribute setting</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;QualityAttributeMeasurement">
		<rdfs:subClassOf rdf:resource="&bmic-monitoringandcontrol;ParameterMeasurement"/>
		<rdfs:label xml:lang="en-US">quality attribute measurement</rdfs:label>
		<iof-av:adaptedFrom>https://database.ich.org/sites/default/files/Q10%20Guideline.pdf</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter measurement of a molecular or product characteristic that is selected for its ability to help indicate the quality of the product.</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;QualityPlan">
		<rdfs:subClassOf rdf:resource="&iof-core;PlanSpecification"/>
		<rdfs:label xml:lang="en-US">quality plan</rdfs:label>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">plan specification to meet regulations that the finished drug product has the required identity and purity characteristics</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;SamplePlan">
		<rdfs:subClassOf rdf:resource="&iof-core;ActionSpecification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;prescribes"/>
				<owl:someValuesFrom rdf:resource="&bmic-monitoringandcontrol;SamplingProcess"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">sample plan</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<skos:altLabel>sampling plan</skos:altLabel>
		<iof-av:adaptedFrom>itl.nist.gov/div898/handbook/ppc/section3/ppc33.htm</iof-av:adaptedFrom>
		<iof-av:explanatoryNote>sampling plans should be designed in such a way that the resulting data will contain a representative sample of the parameters of interest and allow for all questions, as stated in the goals, to be answered.</iof-av:explanatoryNote>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">plan specification with a detailed outline of which measurements will be taken at what times, on which material, in what manner, and by whom</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;SamplingProcess">
		<rdfs:subClassOf rdf:resource="&iof-core;PlannedProcess"/>
		<rdfs:label xml:lang="en-US">sampling process</rdfs:label>
		<dc:contributor>Merck KGaA</dc:contributor>
		<skos:altLabel>sample process</skos:altLabel>
		<iof-av:adaptedFrom>https://www.sciencedirect.com/topics/engineering/sampling-process</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">planned process of acquiring a limited quantity of something which is intended to be similar to and represent a larger amount of that thing(s)</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;StorageTemperature">
		<rdfs:subClassOf rdf:resource="&iof-core;MeasurementInformationContentEntity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;describes"/>
				<owl:someValuesFrom>
					<owl:Class>
						<owl:unionOf rdf:parseType="Collection">
							<rdf:Description rdf:about="&bmic-material;Consumable">
							</rdf:Description>
							<rdf:Description rdf:about="&bmic-material;PortionOfMaterial">
							</rdf:Description>
						</owl:unionOf>
					</owl:Class>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">storage temperature</rdfs:label>
		<terms:contributor xml:lang="en">Merck KGaA</terms:contributor>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">measurement information content entity for the temperature at which a portion of material was stored, generally between ISA-88 recipe components</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:Class rdf:about="&bmic-monitoringandcontrol;SystemParameterConfiguration">
		<rdfs:subClassOf rdf:resource="&bmic-monitoringandcontrol;ParameterConfiguration"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&bfo;BFO_0000164"/>
				<owl:someValuesFrom rdf:resource="&bmic-parameter;SystemParameterSpecification"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en-US">system parameter configuration</rdfs:label>
		<iof-av:adaptedFrom>http://purl.allotrope.org/ontologies/quality#AFQ_0000036</iof-av:adaptedFrom>
		<iof-av:adaptedFrom>http://purl.allotrope.org/voc/afo/REC/2022/03/afo</iof-av:adaptedFrom>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">parameter configuration of a system that concretizes some system setting</iof-av:naturalLanguageDefinition>
	</owl:Class>
	
	<owl:DatatypeProperty rdf:about="&bmic-monitoringandcontrol;hasDateTimeValue">
		<rdfs:label xml:lang="en-US">has date time value</rdfs:label>
		<rdfs:domain rdf:resource="&bfo;BFO_0000203"/>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">data property to store the date time value of a temporal instant</iof-av:naturalLanguageDefinition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&bmic-monitoringandcontrol;hasProcessEndDatetime">
		<rdfs:label xml:lang="en-US">has process end datetime</rdfs:label>
		<rdfs:domain rdf:resource="&bfo;BFO_0000015"/>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">data property to store the end datetime of a process end datetime</iof-av:naturalLanguageDefinition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty rdf:about="&bmic-monitoringandcontrol;hasProcessStartDatetime">
		<rdfs:label xml:lang="en-US">has process start datetime</rdfs:label>
		<rdfs:domain rdf:resource="&bfo;BFO_0000015"/>
		<rdfs:range rdf:resource="&xsd;dateTime"/>
		<iof-av:naturalLanguageDefinition xml:lang="en-US">data property to store the start datetime of a process end datetime</iof-av:naturalLanguageDefinition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&bmic-monitoringandcontrol;hasQuantity">
		<rdfs:subPropertyOf rdf:resource="&qudt;hasQuantity"/>
		<rdfs:label xml:lang="en-US">has quantity</rdfs:label>
		<rdfs:domain rdf:resource="&iof-core;MeasurementInformationContentEntity"/>
		<rdfs:range rdf:resource="&qudt;Quantity"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&bmic-monitoringandcontrol;hasTimestamp">
		<rdfs:label xml:lang="en-US">has Timestamp</rdfs:label>
		<rdfs:domain rdf:resource="&bmic-monitoringandcontrol;QualityAttributeMeasurement"/>
		<rdfs:range rdf:resource="&bfo;BFO_0000203"/>
	</owl:ObjectProperty>

</rdf:RDF>