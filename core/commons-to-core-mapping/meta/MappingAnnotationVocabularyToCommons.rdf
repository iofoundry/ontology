<rdf:RDF xml:base="https://spec.industrialontologies.org/ontology/core/meta/MappingAnnotationVocabularyToCommons/"
	xmlns:cmns-av="https://www.omg.org/spec/Commons/AnnotationVocabulary/"
	xmlns:dct="http://purl.org/dc/terms/"
