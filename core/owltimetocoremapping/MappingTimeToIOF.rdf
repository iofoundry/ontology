<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY dcterms "http://purl.org/dc/terms/">
	<!ENTITY iof-av "https://spec.industrialontologies.org/ontology/annotation/">
	<!ENTITY iof-constr "https://spec.industrialontologies.org/ontology/construct/">
	<!ENTITY map-time-iof "https://spec.industrialontologies.org/ontology/core/commonstocoremapping/MappingOWLTimeToIOF/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.industrialontologies.org/ontology/core/commonstocoremapping/MappingOWLTimeToIOF/"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:iof-av="https://spec.industrialontologies.org/ontology/annotation/"
	xmlns:iof-constr="https://spec.industrialontologies.org/ontology/construct/"
	xmlns:map-time-iof="https://spec.industrialontologies.org/ontology/core/commonstocoremapping/MappingOWLTimeToIOF/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.industrialontologies.org/ontology/core/commonstocoremapping/MappingOWLTimeToIOF/">
		<rdfs:label xml:lang="en">Mapping OWL Time ontology to IOF Core ontology</rdfs:label>
		<dcterms:abstract xml:lang="en">This ontology maps some concepts of the OWL Time (https://www.w3.org/TR/owl-time/) to corresponding concepts of IOF Core to enable IOF Core to associate calendar date and clock time to a Time Instant as well as a measurement of duration to a Time Interval based on various reference systems, e.g., calendar and clock systems) and units, e.g., hour, minute, second.</dcterms:abstract>
		<dcterms:contributor>Jim Logan, No Magic, Inc.</dcterms:contributor>
		<dcterms:contributor>Milos Drobnjakovic, National Institute of Standards and Technology (NIST)</dcterms:contributor>
		<dcterms:creator>Arkopaul Sarkar, École Nationale d’Ingénieurs de Tarbes (ENIT)</dcterms:creator>
		<dcterms:license rdf:datatype="&xsd;anyURI">http://opensource.org/licenses/MIT</dcterms:license>
		<dcterms:publisher xml:lang="en">Industrial Ontology Foundry</dcterms:publisher>
		<dcterms:title>Industrial Ontology Foundry (IOF) Mapping the OWL Time (W3C) to the IOF Core Ontology</dcterms:title>
		<owl:imports rdf:resource="http://www.w3.org/2006/time#2016"/>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/"/>
		<owl:versionIRI rdf:resource="https://spec.industrialontologies.org/ontology/202502/core/commonstocoremapping/MappingOWLTimeToIOF/"/>
		<iof-av:copyright>Copyright (c) 2022, 2023, 2024, 2025 Open Applications Group</iof-av:copyright>
	</owl:Ontology>
	
	<owl:Class rdf:about="http://www.w3.org/2006/time#TemporalDuration">
		<owl:equivalentClass rdf:resource="&iof-constr;TemporalDurationValueExpression"/>
	</owl:Class>
	
	<owl:Class rdf:about="&iof-constr;TemporalInstantValueExpression">
		<rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#TemporalPosition"/>
	</owl:Class>

</rdf:RDF>