								<owl:onProperty rdf:resource="&bfo;BFO_0000056"/>
								<owl:someValuesFrom rdf:resource="&iof-core;ProductProductionProcess"/>
							</owl:Restriction>
						</owl:intersectionOf>
					</owl:Class>
