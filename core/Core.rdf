												<owl:someValuesFrom>
													<owl:Class>
														<owl:intersectionOf rdf:parseType="Collection">
															<rdf:Description rdf:about="&bfo;BFO_0000203">
															</rdf:Description>
															<owl:Class>
																<owl:intersectionOf rdf:parseType="Collection">
																	<owl:Restriction>
