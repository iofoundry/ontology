	<owl:Class rdf:about="&iof-core;Manufacturer">
		<rdfs:subClassOf rdf:resource="&iof-core;Agent"/>
		<rdfs:label xml:lang="en-us">manufacturer</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
