	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/InformationSlot/ComponentSlot">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/InformationSlot/InformationSlot"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/InformationSlot/filledBy"/>
				<owl:allValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/InformationSlot/describes"/>
						<owl:someValuesFrom rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/MaterialComponent"/>
					</owl:Restriction>
				</owl:allValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/InformationSlot/slotOf"/>
				<owl:allValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/InformationSlot/describes"/>
						<owl:someValuesFrom>
							<owl:Class>
								<owl:intersectionOf rdf:parseType="Collection">
									<rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000004">
									</rdf:Description>
									<owl:Class>
										<owl:complementOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000006"/>
									</owl:Class>
								</owl:intersectionOf>
							</owl:Class>
						</owl:someValuesFrom>
					</owl:Restriction>
				</owl:allValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">component slot</rdfs:label>
