	</owl:Class>
	
	<owl:Class rdf:about="&rel;IndependentContinuantAndNotSpatialRegion">
		<rdfs:label>Independent Continuant and not Spatial Region</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000004">
					</rdf:Description>
					<owl:Class>
						<owl:complementOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000006"/>
					</owl:Class>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
	</owl:Class>
	
	<owl:Class rdf:about="&rel;MonetaryObject">
		<rdfs:label>Monetary Object</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000004">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/hasRole"/>
						<owl:someValuesFrom rdf:resource="&rel;MonetaryObjectRole"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
	</owl:Class>
	
	<owl:Class rdf:about="&rel;MonetaryObjectRole">
		<rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000023"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;usedMonetaryObjectIn"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Monetary Object Role</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&rel;NegotiatedPurchase">
		<rdfs:label>Negotiated Purchase</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&rel;Purchase">
		<rdfs:subClassOf rdf:resource="&rel;Relator"/>
