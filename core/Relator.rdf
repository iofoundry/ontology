	<owl:ObjectProperty rdf:about="&rel;sellerIn">
		<rdfs:subPropertyOf rdf:resource="&rel;summingRelator"/>
		<rdfs:label>seller in</rdfs:label>
		<rdfs:domain rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/SupplierRole"/>
		<rdfs:range rdf:resource="&rel;Purchase"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&rel;summedMode">
		<rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000111"/>
