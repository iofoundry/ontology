<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY rel "https://spec.industrialontologies.org/ontology/core/Relator/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.industrialontologies.org/ontology/core/Relator/"
	xmlns="https://spec.industrialontologies.org/ontology/core/Relator/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:rel="https://spec.industrialontologies.org/ontology/core/Relator/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.industrialontologies.org/ontology/core/Relator/">
		<rdfs:label>Relator</rdfs:label>
		<owl:imports rdf:resource="http://purl.obolibrary.org/obo/bfo.owl"/>
	</owl:Ontology>
	
	<owl:Class rdf:about="&rel;ExternallyDependentMode">
		<rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000197"/>
				<owl:allValuesFrom rdf:resource="&rel;IndependentContinuantAndNotSpatialRegion"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000137"/>
				<owl:allValuesFrom rdf:resource="&rel;Relator"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000197"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000137"/>
				<owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;externallyDependsOn"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Externally Dependent Mode</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&rel;IndependentContinuantAndNotSpatialRegion">
		<rdfs:label>Independent Continuant and not Spatial Region</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000004">
					</rdf:Description>
					<owl:Class>
						<owl:complementOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000006"/>
					</owl:Class>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
	</owl:Class>
	
	<owl:Class rdf:about="&rel;Relator">
		<rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;foundedBy"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;mediates"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;summedMode"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Relator</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&rel;externallyDependsOn">
		<rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000195"/>
		<rdfs:label>externally depends on</rdfs:label>
		<rdfs:domain rdf:resource="&rel;ExternallyDependentMode"/>
		<rdfs:range rdf:resource="&rel;IndependentContinuantAndNotSpatialRegion"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&rel;foundedBy">
		<rdfs:label>founded by</rdfs:label>
		<rdfs:domain rdf:resource="&rel;Relator"/>
		<rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&rel;mediates">
		<rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000195"/>
		<rdfs:label>mediates</rdfs:label>
		<rdfs:domain rdf:resource="&rel;Relator"/>
		<rdfs:range rdf:resource="&rel;IndependentContinuantAndNotSpatialRegion"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&rel;summedMode">
		<rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000111"/>
		<rdfs:label>summed mode</rdfs:label>
		<rdfs:domain rdf:resource="&rel;Relator"/>
		<rdfs:range rdf:resource="&rel;ExternallyDependentMode"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&rel;summingRelator">
		<rdfs:label>summing relator</rdfs:label>
		<rdfs:domain rdf:resource="&rel;ExternallyDependentMode"/>
		<rdfs:range rdf:resource="&rel;Relator"/>
	</owl:ObjectProperty>

</rdf:RDF>