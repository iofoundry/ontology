	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&rel;usedMonetaryObject">
		<rdfs:subPropertyOf rdf:resource="&rel;summedMode"/>
		<rdfs:label>used monetary object</rdfs:label>
		<rdfs:domain rdf:resource="&rel;Purchase"/>
		<rdfs:range rdf:resource="&rel;MonetaryObjectRole"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&rel;usedMonetaryObjectIn">
		<rdfs:subPropertyOf rdf:resource="&rel;summingRelator"/>
