			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;mediates"/>
				<owl:onClass rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/Product"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;mediates"/>
				<owl:onClass rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/Supplier"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;mediates"/>
				<owl:onClass rdf:resource="&rel;MonetaryObject"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/isOutputOfAtSomeTime"/>
				<owl:onClass rdf:resource="&rel;SellingProcess"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Purchase</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&rel;Relator">
		<rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;foundedBy"/>
				<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;mediates"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&rel;summedMode"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Relator</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&rel;SellingProcess">
		<rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
