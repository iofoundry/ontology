		<rdfs:domain rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/CustomerRole"/>
		<rdfs:range rdf:resource="&rel;Purchase"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&rel;externallyDependsOn">
