<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY iof-core "https://spec.industrialontologies.org/ontology/core/Core/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://spec.industrialontologies.org/ontology/core/CoreProposal/"
	xmlns="https://spec.industrialontologies.org/ontology/core/CoreProposal/"
	xmlns:iof-core="https://spec.industrialontologies.org/ontology/core/Core/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/">
		<rdfs:label>Core Proposal</rdfs:label>
		<owl:imports rdf:resource="http://purl.obolibrary.org/obo/bfo.owl"/>
		<owl:imports rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/"/>
	</owl:Ontology>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/BlockingOfDisposition">
		<rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/CoreProposal/blocks"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="https://spec.industrialontologies.org/ontology/core/CoreProposal/causedBy"/>
				<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Blocking of Disposition</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/EmergenceOfDisposition">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/CoreProposal/GainOfDisposition"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;hasInput"/>
				<owl:onClass rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Emergence of Disposition</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/FailureMode">
		<rdfs:label>Failure Mode</rdfs:label>
		<owl:equivalentClass rdf:resource="https://spec.industrialontologies.org/ontology/core/CoreProposal/LossOfCapability"/>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/GainOfDisposition">
		<rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;hasOutput"/>
				<owl:onClass rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Gain of Disposition</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/LossOfCapability">
		<rdfs:subClassOf rdf:resource="https://spec.industrialontologies.org/ontology/core/CoreProposal/LossOfDisposition"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;hasInput"/>
				<owl:someValuesFrom rdf:resource="&iof-core;Capability"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Loss of Capability</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/LossOfDisposition">
		<rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&iof-core;hasInput"/>
				<owl:onClass rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>Loss of Disposition</rdfs:label>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/blockedBy">
		<rdfs:label>blocked by</rdfs:label>
		<rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
		<rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
		<owl:inverseOf rdf:resource="https://spec.industrialontologies.org/ontology/core/CoreProposal/blocks"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/blocks">
		<rdfs:subPropertyOf rdf:resource="&iof-core;hasInput"/>
		<rdfs:label>blocks</rdfs:label>
		<rdfs:domain>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000016">
					</rdf:Description>
					<rdf:Description rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/BlockingOfDisposition">
					</rdf:Description>
				</owl:unionOf>
			</owl:Class>
		</rdfs:domain>
		<rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/causedBy">
		<rdfs:subPropertyOf rdf:resource="&iof-core;hasInput"/>
		<rdfs:label>caused by</rdfs:label>
		<rdfs:domain rdf:resource="https://spec.industrialontologies.org/ontology/core/CoreProposal/BlockingOfDisposition"/>
		<rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/contributesTo">
		<rdfs:label>contributes to</rdfs:label>
		<rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
		<rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
		<owl:inverseOf rdf:resource="https://spec.industrialontologies.org/ontology/core/CoreProposal/emergesFrom"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="https://spec.industrialontologies.org/ontology/core/CoreProposal/emergesFrom">
		<rdfs:label>emerges from</rdfs:label>
		<rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
		<rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
	</owl:ObjectProperty>

</rdf:RDF>